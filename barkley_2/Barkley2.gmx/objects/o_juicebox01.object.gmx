if (scr_quest_get_state("comServ") &gt;= 7) then scr_event_interactive_deactivate();
if (scr_time_db("tnnCurfew") != "before") then scr_event_interactive_deactivate(); 

scr_event_hook_onInteract(event_cinema);

scr_entity_animation_new("default");
scr_entity_animation_setRange("default", 0, 12); 
scr_entity_animation_setSpeed("default", 0);

timerfrm =0;
animfrm = 0;
animInterv = 6;
curImg = 0;
timer_delay = 5;
audioemit = audio_emitter_create();

script = "
IF body == governor | GOTO | GOVERNOR
IF comServ == 2     | GOTO | COM2
IF comServ == 4     | GOTO | COM4
IF comServ == 6     | GOTO | COM6
GOTO | NORMAL

GOVERNOR
DIALOG | Juicebox | Heh heh... Evening, Mr. Governor...

COM6
DIALOG | Juicebox | Granny uhh... Forgot her medicine.

COM4
DIALOG | Juicebox | Hey kid. Cornrow's teaching me about uh, numbers and stuff.

COM2
DIALOG | Juicebox | Cornrow's teaching me to read, uh, words.

NORMAL
DIALOG | Juicebox | Uhh, hey kid.
";

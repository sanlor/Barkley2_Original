if (paused()) exit;

if(timerfrm&lt;animInterv)
{
    timerfrm+=5*dt();
}
else
{
    timerfrm -=animInterv;
    if(animfrm&gt;0){animfrm-=5*dt();}
    else
    {
        image_index+=1;
        curImg = image_index;
        event_user(5);
    }
}

// Coin Flip sound //
if _active = true then
    {
    if timer_delay &gt; 0 then
        {
        timer_delay -= dt();
        if timer_delay &lt;= 0 then 
            {
            audio_emitter_falloff(audioemit,80,200,1);
            audio_emitter_gain(audioemit,0.5 * global.sfx_gain_master);
            audio_emitter_position(audioemit,x,y,0);
            
            }
        }
    else
        {
        var guplur = image_index;
        if guplur = 3 and !audio_is_playing_ext("sn_juicebox_coinflip") then audio_play_sound_on_ext(audioemit,"sn_juicebox_coinflip",false,80);  
        }
    } 

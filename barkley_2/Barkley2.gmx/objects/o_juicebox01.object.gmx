switch(curImg)
{
    case 1: ///blinking or flipping?
        if(random(10)&lt;6){curImg = 2;} ///a bit more likely to flip than to blink
        break;
        
    case 2: ///blinked.
        if(random(10)&lt;2){curImg = 1; animfrm = 1 + irandom(1);} ///do two blinks in a row
        else {animfrm = 1 + irandom(4); curImg = 0;} ///wait before next flip/blink
        break;
        
    case 12:
        if(random(10)&lt;2){curImg = 2;} ///do two flips in a row
        else {animfrm = 1 + irandom(8); curImg = 0;} ///wait before next flip/blink
        break;
}

image_index = curImg;
scr_entity_animation_setFrame("default",curImg);

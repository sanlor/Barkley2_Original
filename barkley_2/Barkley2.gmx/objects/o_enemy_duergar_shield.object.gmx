        n.image_xscale = atk.image_xscale;
        n.image_yscale = atk.image_yscale;
        n.image_index = atk.image_index;
        n.image_angle = n.direction;
        n.spd = atk.move_dist;
    }
    
    // Bullet bounce SFX //
    audio_play_sound_AI(shield_sound);
}

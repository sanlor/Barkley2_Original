            <string>// Fly around //
if hitpoints &lt;= 0 then
    {
    x += speed_x;
    y += speed_y;
    if speed_y &lt; 5 then speed_y += 0.15;
    image_angle += rotation_speed;
    }

// Cut //
if place_meeting(x, y, o_mg_organ_cutter) then
    {
    if scr_input_check_held(KEYID_ACTION, PLAYER1) then 
        {
        hitpoints -= 1;
        if audio_pause &lt;= 0 then 
            {
            if !audio_is_playing_ext("sn_pdt_wilhelm01") then audio_play_sound_at_xy("sn_pdt_wilhelm01", false, 5, x, y);
            audio_play_sound_at_xy("organ_harvest_skin", false, 5, x, y);
            audio_pause = random(1) + 0.25;
            }
        if blood_cooldown &lt;= 0 then 
            {
            instance_create(o_mg_organ_cutter.x, o_mg_organ_cutter.y, o_mg_organ_blood);
            blood_cooldown = 2 + irandom(2);
            }
        with o_mg_organ_hubert_body image_index += 0.1;
        with o_mg_organ_hubert_head 
            {
            timer_pain = 5;
            hurt = true;
            }
        with o_mg_organ_system 
            {
            bloodied_cutter = true;
            babby_cut = true;
            }
        }
    }
    
if audio_pause &gt; 0 then audio_pause -= dt();
if blood_cooldown &gt; 0 then blood_cooldown -= 1;

// out of sight, gone //
if y &gt;= view_yview + 240 then instance_destroy();

</string>

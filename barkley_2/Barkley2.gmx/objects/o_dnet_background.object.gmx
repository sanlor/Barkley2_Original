// Backdrop effects // ----------------------------------------------------------------------------- //
// ---------------------------------------------------------------------------------------- //
backdrop_x += lengthdir_x(backdrop_speed, backdrop_direction) * dt();
backdrop_y += lengthdir_y(backdrop_speed, backdrop_direction) * dt();

// Backdrop animation //
if backdrop_image_max != 0 then
    {
    // Animation goes forward //
    backdrop_image_index += backdrop_image_speed * dt();
    
    // Loop back //
    if backdrop_image_index &gt; backdrop_image_max then backdrop_image_index -= backdrop_image_max;
    }

// Change backdrop direction //
backdrop_direction += backdrop_direction_change * dt();
if backdrop_direction &lt; 180 then backdrop_speed += 0.06 * dt();
else backdrop_speed -= 0.06 * dt();

// Direction limit //
if backdrop_direction &gt; 360 then backdrop_direction -= 360;

// Speed limit //
if backdrop_speed &gt; 4.5 then backdrop_speed = 4.5;
else if backdrop_speed &lt; -4.5 then backdrop_speed = -4.5;

// Change direction //
if timer_backdrop_direction &gt; 0 then
    {
    // Tick down //
    timer_backdrop_direction -= dt();
    
    // Event //
    if timer_backdrop_direction &lt;= 0 then
        {
        timer_backdrop_direction = irandom(30) + 20;
        backdrop_direction_change = random(12) - random(12);
        }
    }
</string>

            <string>///Ladder BS
if (instance_exists(o_hoopz) == 0) exit;
// Death handling
var life, dead;
dead = 0;
life = scr_stats_getCurrentStat(o_hoopz, STAT_CURRENT_HP);
if (life &lt; 0 and o_hoopz.stance != scr_player_stance_death) {
    if(global.godMode)
    {
        //scr_stats_setCurrentStat(self,STAT_CURRENT_HP,scr_stats_getEffectiveStat(self,STAT_EFFECTIVE_MAX_HP));
        
    }
    else
    {
        dead = 1;
    }
}
if (dead) exit;
if (place_meeting(x, y, o_hoopz))
{
    //yPush = (o_hoopz.yprevious - o_hoopz.y) * global.stairUD;
    //Only assign once
    global.tObj = id;
    
    if scr_input_check_held(KEYID_UP, PLAYER1) or scr_input_check_held(KEYID_DOWN, PLAYER1) then
        {
        if sfx &gt; 0 then 
            {
            sfx -= dt();
            if sfx &lt;= 0 then 
                {
                audio_play_sound_at_xy(ladder_sound, false, 10, x, y);
                sfx = 3;
                }
            }
        }
    else sfx = 1;
    
    with (o_hoopz)
    {
        global.tObj.stepInto += dt();
        if (global.tObj.stepInto &lt; (10 * global.ladderDelay) || BodySwap() != "hoopz")
        {
            o_hoopz.x = global.tObj.bx;
            o_hoopz.y = global.tObj.by;
        }
        else
        {
            if (stance != scr_player_stance_climb)
            {
                scr_player_setStance(scr_player_stance_climb);
                lock_stance = 1;
            }
        }
    }
}
else stepInto = 0;


exit;
if (yPush != 0)
{
    with (o_hoopz)
    {
        scr_collision_move_contact_solid(0, 0 + yPush, collision_check);
        
    }
}

if (o_hoopz.stance != scr_player_stance_climb)
    {
        o_hoopz.lock_stance = 1;
        scr_player_setStance(scr_player_stance_climb);
    }
</string>

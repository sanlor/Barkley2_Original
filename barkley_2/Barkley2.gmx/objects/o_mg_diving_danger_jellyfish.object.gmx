        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Pause
if (paused()) exit;

if (falling == 0) sub += 1.5 * dt();
if (sub &gt; 4)
{
    y_speed = -1.25;
    facing = -facing;
    x_speed = facing;
    sub = 0;
}

// Drop on ceilings //
if !place_free(x, y - 8) then 
    {
    y += 1;
    falling = 1;
    x_speed = 0;
    y_speed = 0;
    }

// Start kicking at the bottom //
if !place_free(x, y + 8) then 
    {
    y -= 1;
    falling = 0;
    }

// Movement //
x += x_speed * 30 * dt_sec();
y += y_speed * 30 * dt_sec();
    
// Friction //
if (falling == 0)
{
    if x_speed &gt; 0 then x_speed -= 0.025 * 60 * dt_sec();
    if x_speed &lt; 0 then x_speed += 0.025 * 60 * dt_sec();
}

// Gravity //
y_speed += 0.05 * 30 * dt_sec();

// Limit //
if y_speed &gt; 1 then y_speed = 1;

// Killed by torpedo //
if place_meeting(x, y, o_mg_diving_danger_torpedo) then with instance_nearest(x, y, o_mg_diving_danger_torpedo) instance_destroy();
if place_meeting(x, y, o_mg_diving_danger_explosion) then instance_destroy();

// Image //
image_xscale = -facing;
</string>

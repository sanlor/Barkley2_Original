            <string>/// Calculate shank and auto-escape
if (Quest("gutterhoundArrest") == 1)
{
    if (Quest("gutterhoundQuest") &gt;= 5) Quest("gutterhoundPrisonEscape", 1);
}

var enemies = 0;

// Pelekryte
if (Quest("pelekryteState") != 5) 
{
    if (Quest("pelekryteState") == 2 || Quest("pelekryteState") == 3 || Quest("pelekryteState") == 4 || Quest("pelekryteState") == 6) 
    {
        enemies += 1;
        Quest("pelekryteCanShank", 1);
    }
}

// Gutterhound
// If you know of his scheme but dont help he dislikes you
// 2 = You refuse Gutterhound's offer.
// 3 = You've refused Gutterhound's offer twice. Can't get it again.
// 8 = Gutterhound robbed the bank without you
//if (Quest("knowGutterhoundRobbery") &gt;= 1)
//{
//    var gut = Quest("gutterhoundQuest");
//    if (gut == 2 || gut == 3 || gut == 8) enemies += 1;
//}

if (Quest("sureshArrest") == 1) 
{
    enemies += 1;
    Quest("sureshCanShank", 1);
}
if (Quest("moriartyArrest") == 1) 
{
    // auto
    enemies = 100;
    Quest("moriartyCanShank", 1);
}
if (Quest("kunigundeArrest") == 1) 
{
    enemies += 1;
    Quest("kunigundeCanShank", 1);
}

Quest("prisonEnemies", enemies);

if (enemies == 0) Quest("prisonShankTime", 99); // Never get shanked
else Quest("prisonShankTime", abs(enemies - 3) + 1);
if (Quest("moriartyArrest") == 1) Quest("prisonShankTime", 0);

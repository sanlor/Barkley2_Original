                scr_event_build_dialogue("Phileass Fog", "s_port_phileass", "... Uhh... Ben...?");
                scr_event_build_dialogue("Benjamin-Hur", "s_port_benhur", "It is alright Phil, your brain is still recalibrating itself for your new body. The pain will subside soon enough. Do not worry Phil, I am here for you.");
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass", "Wha... bo... dy...?");
                scr_event_build_dialogue("Benjamin-Hur", "s_port_benhur", "Yes Phil. I have remanimated you as the Ultimate Being!");
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass", "Wha... you... do... Ben...?");
                scr_event_build_dialogue("Benjamin-Hur", "s_port_benhur", "I had to do it Phil! I had to get you back. Oh Phil, you have no idea how far I have gone to be with you again. I... I almost can not believe that you are really there. It is... It all seems too good to be true, but I know that this is all real. This is all possible only through my reanimative science!");
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass", "Why did... you... do this...?");
                scr_event_build_dialogue("Benjamin-Hur", "s_port_benhur", "Why? Because I love you Phil. I could not stand another day without you. You would not believe what I... all those long days and sleepless nights that I worked on a way to bring you back from the dead. All the evils that I have had to commit. It was all worth it. Heh, looks like matrimony is back on the menu!");
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass", "I... died?");
                scr_event_build_dialogue("Benjamin-Hur", "s_port_benhur", "Yes Phil. The medicine stopped working and you slipped into a coma. No matter what I did your condition got worse each day until... I put your body into a cryochamber, to stop deterioration. I scrapped all my Big Bad Wolf &amp; Benjamin Biotek projects and went on to work for a cure for death. Everyone told me that it was a fools errand. They thought I had gone mad with grief. But they were wrong! They were the fools, not me!");
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass", "I'm a... freak... a monster. What have you... done Ben?");
                scr_event_build_dialogue("Benjamin-Hur", "s_port_benhur", "No, you are not a monster! You are the Ultimate Being! The result of Project Birchman! You are not a monster Phil, you are science triumphing over death itself.");
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass", "Project Birchman...?");
                scr_event_build_dialogue("Benjamin-Hur", "s_port_benhur", "I used Pinocchios DNA to imbue your body with his amazing regenerative powers! You remember Pinocchio, right? That boy from Ys-Kolob who was spawned from a block of firewood. That boy whos nose grew in length in direct relation to the malignancy of his lies. You remember right?"); 
                scr_event_build_animation_play_and_set(o_pdt_phileass01, "standup", "standing");
                scr_event_build_animation_set(o_pdt_biotek_benhur01, "look");
                scr_event_build_wait_for_actions();
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass", "I'm... in Pinocchios... body??");
                scr_event_build_dialogue("Benjamin-Hur", "s_port_benhur", "Well, yes and no. I fused his body into yours to repair you, but... but it doesn't matter Phil. You are here with me now! I... I love you Phil. I love you with all my heart. Do you still love me as well, Phil?");
                scr_event_build_music_set("mus_blankTEMP", 0);
                scr_event_build_wait(1);
                scr_event_build_dialogue("Benjamin-Hur", "s_port_benhur", "P-Phil...?");
                scr_event_build_camera_move_to_point(421, 270, MOVE_NORMAL);
                scr_event_build_move_to_point_ext(o_pdt_biotek_benhur01, 404, 288, MOVE_FAST);
                scr_event_build_wait_for_actions();
                scr_event_build_animation_set(o_pdt_biotek_benhur01, "backside");
                scr_event_build_dialogue("Benjamin-Hur", "s_port_benhur", "Look me in the eyes and say it Phil! Do you still love me, Phil?! Please, I have to hear you say it! Everything I did was to hear you say those words again... Phil, do you still love me?");
                scr_event_build_wait(1.5);
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass", "...YES!");
                
                // Nose impalement //
                scr_event_build_animation_play_and_set(o_pdt_phileass01, "impale", "hold");
                scr_event_build_wait(0.25);
                scr_event_build_sound_play("sn_pdt_noseslash01");
                scr_event_build_wait(0.75);
                scr_event_build_animation_play_and_set(o_pdt_biotek_benhur01, "impaled", "hurt");
                scr_event_build_wait(1);
                scr_event_build_dialogue("Benjamin-Hur", ""s_port_benhur_dying", "Aaaaaaaaaaaaaaarrgghh!!!");
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass_nose", "Did you really think I ever loved you?");
                scr_event_build_dialogue("Benjamin-Hur", ""s_port_benhur_dying", "...Agh... But... How? This is... impossible... My calculations... The simulation said... that you... love me...");
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass_nose", "You are a fool, Ben. There was never love between us... only lust.");
                scr_event_build_dialogue("Benjamin-Hur", ""s_port_benhur_dying", "...Guh... but... I... did everything for... you... ");
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass_nose", "There is only one person I've ever truly loved Ben. Only one.");            
                scr_event_build_animation_play_and_set(o_pdt_phileass01, "retract", "angry");
                scr_event_build_animation_play_and_set(o_pdt_biotek_benhur01, "death", "dead");
                scr_event_build_wait(2);
                
                // Phileass leaves //
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass", "My sweet Passepartout...");
                scr_event_build_wait(1);
                scr_event_build_dialogue("Benjamin-Hur", ""s_port_benhur_dying", "...");
                scr_event_build_wait(1);
                scr_event_build_dialogue("Phileass Fog", "s_port_phileass", "...Passepartout... The journey is finally over. I'm coming home.");         
                scr_event_build_move_to_point_ext(o_pdt_phileass01, 320, 440, MOVE_SLOW);
                scr_event_build_wait_for_actions();
                scr_event_build_destroy_object(o_pdt_phileass01);
                scr_event_build_wait(1);
    
                // Fade out //
                scr_event_build_quest_state("pdt_cyberdoor", 0);
                scr_event_build_quest_state("pdt_benhur", 1);
                scr_event_build_quest_state("pdt_cowardlylion", 4);
                scr_event_build_music_set("mus_blankTEMP", 0);            
                scr_event_build_wait(0.5);
                scr_event_build_teleport(r_pdt_respawn01, 256, 180, 2, 2);    
            }
        scr_event_advance(event); 
        }
        }
    } 
    

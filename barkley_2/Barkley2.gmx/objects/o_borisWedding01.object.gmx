IF janeWedding      | IF borisJane = 0      | GOTO | JANE_HAVE
IF dinahWedding     | IF borisDinah = 0     | GOTO | DINAH_HAVE
IF guillaumeWedding | IF borisGuillaume = 0 | GOTO | GUILLAUME_HAVE

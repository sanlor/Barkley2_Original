        <arguments>
          <argument>
            <kind>1</kind>
            <string>var gun = scr_equipment_guns_getCurrentGun();

if (instance_exists(o_hud)) {
    x = 0;
    y = 0;
}

if (gun != NULL)
{
    ////Weapon name and description.
    draw_set_font(global.fn_small)
    draw_set_alpha(1);
    draw_set_color(c_white);
    draw_set_halign(fa_left);
    draw_set_valign(fa_top);
    
    ////debug current gun info
    draw_text(x+5,y-24, string_upper(GunMap(gun, "model")));
    adish = "";
    if(gun[? "pShots"]&gt;1){adish = " X"+string(gun[? "pShots"]);}
    _bot = floor(gun[? "pDamageMin"]);
    _top = floor(gun[? "pDamageRand"]+gun[? "pDamageMin"]);
    _mix= string(_bot);
    if(_bot != _top){_mix += " - " + string(_top);}
    
    draw_text(x+10,y+300,"DMG "+_mix +adish);
    draw_text(x+10,y+310,"ACC "+string(gun[? "pAccuracy"]) + " DEGREES");
    draw_text(x+10,y+320,"RNG "+string(gun[? "pRange"]));

    blPerSec = (((15 + power(1 + gun[? "pFireSpeed"],1.75) / 4 )/gun[? "pFireTimerTarget"])*60) * gun[? "pBurstAmount"] * gun[? "pAmmoCost"];
    
    draw_text(x+10,y+330,"SPD "+string(gun[? "pFireSpeed"]) + " (" + string(blPerSec) + " SHOTS PER SEC )");
    draw_text(x+10,y+340,"WEIGHT "+ string(gun[? "pWeight"]));
    
    draw_text(x+10,y+80,"CORE STATS ");
    draw_text(x+10,y+90,"PWR "+string(gun[? "sPower"])+ " x " + string(gun[? "pPowerMod"]) + "-" + string(gun[? "pPowerMaxMod"]));
    draw_text(x+10,y+100,"SPD "+string(gun[? "sSpeed"])+ " x " + string(gun[? "pSpeedMod"]));
    draw_text(x+10,y+110,"RNG "+string(gun[? "sRange"])+ " x " + string(gun[? "pRangeMod"]));
    draw_text(x+10,y+120,"AMM "+string(gun[? "sAmmo"])+ " x " + string(gun[? "pAmmoMod"]));
    draw_text(x+10,y+130,"AFX "+string(gun[? "sAffix"])+ " x " + string(gun[? "pAffixMod"]));
    draw_text(x+10,y+140,"WGT "+string(gun[? "sWeight"])+ " x " + string(gun[? "pWeightMod"]));
    
    draw_text(x+10,y+155,"RARITY " + string(gun[? "rarity"]));
    draw_text(x+10,y+165,"AFFIX PTS " + string(gun[? "pointsUsed"]));
    draw_text(x+10,y+175,"CORE STAT PTS " + string(gun[? "numberval"] - gun[? "pointsUsed"]));
    
    Gunsmap("draw", x + 10, y + 190, 1, Gunsmap("type to index", gun[? "pType"]), gun[? "gunmap_pos"]);
}
</string>

if(timer&gt;0)
{
    if(wobble)
    {
        if(scale&gt;0.6+0.1*dt())
        {
            scale-=0.1*dt();
        }
        else{wobble = false;}
    }
    else
    {
        if(scale&lt;1-0.2*dt())
        {scale+=0.2*dt();}
        else
        {
            scale = 1;
            if(timer&lt;10){wobble = true; image_index = min(image_number-1,image_index+1);}
        }
    }
    
    timer-=dt();
}
else
{
    if(scale&lt;1.5*dt()){scale+=0.15*dt()}
    else
    {
        ////hatch here
        event_user(9);
        instance_destroy();
    }
}


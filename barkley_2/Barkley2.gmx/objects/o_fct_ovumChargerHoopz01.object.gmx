        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Inheritance //
event_inherited();

// Pause //
if paused() exit;

// Sparks //
if timer_smoke &gt; 0 then
    {
    // Tick down //
    timer_smoke -= dt();
    
    // Event //
    if timer_smoke &lt;= 0 then 
        {
        timer_smoke = 0.1 + irandom(1);        
        var vapeSize = choose(1, 0, 2, 3);
        smk = Smoke("puff keep", x + irandom(14) - irandom(14), y - 45 + irandom(5) - irandom(10), 0, 10 + (vapeSize) + random(5 + (vapeSize / 2)));
        smk.move_x = choose(1, 0, -1);
        smk.move_y = -12;
        smk.smoke_speed = 1;
        with (smk) 
            { 
            event_user(0); 
            instance_destroy(); 
            }
        }
    }
    
// Depth //
scr_dny(+100);
</string>

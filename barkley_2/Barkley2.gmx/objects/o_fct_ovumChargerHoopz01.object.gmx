        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// The Influence Ovum Charger
event_inherited();
shadow_visible = 0;
scr_event_hook_onCommon(event_cinema);
scr_actor_unsetRigid(id);
event_user(0);
if ClockTime() &gt;= 5 then timer_smoke = 0;
else timer_smoke = 1;

// Disable during tutorial //
if Quest("tutorialProgress") &lt; 100 then 
    {
    _interactive = false;
    _disableOutline = true;
    }

// Animation States
scr_entity_animation_define("broken_egg", s_fct_ovumCharger01, 0, 2, ANIMATION_DEFAULT_SPEED * 0.4);
scr_entity_animation_define("broken_empty", s_fct_ovumCharger01, 2, 1, 0);
scr_entity_animation_define("empty", s_fct_ovumCharger01, 3, 1, 0);
scr_entity_animation_define("connect", s_fct_ovumCharger01, 4, 1, 0);
scr_entity_animation_define("charged_1", s_fct_ovumCharger01, 5, 1, 0);
scr_entity_animation_define("charged_2", s_fct_ovumCharger01, 6, 1, 0);
scr_entity_animation_define("charged_3", s_fct_ovumCharger01, 7, 1, 0);
scr_entity_animation_define("complete", s_fct_ovumCharger01, 8, 1, 0);

// State Switch
switch (Quest("dynamoeufHoopz"))
    {
    case 0: Animation("set", o_fct_ovumChargerHoopz01, "broken_egg"); break;    
    case 1: Animation("set", o_fct_ovumChargerHoopz01, "broken_empty"); break;
    }
    
// Smoke //
smkCol[0] = make_color_rgb(80, 50, 50);
smkCol[1] = make_color_rgb(90, 40, 40);
smkCol[2] = make_color_rgb(100, 60, 20);
</string>
          </argument>
        </arguments>
      </action>

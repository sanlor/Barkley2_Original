            <string>// Inheritance //
event_inherited();

// Pause //
if paused() exit;

// Animation //
image_index += dt_sec() * 6;

// Spawn pollen //
if pollen_spawn &gt; 0 then pollen_spawn -= dt();
else
    {
    spawn = floor(image_index)
    if spawn = 0 and spawn_able = true then 
        {
        repeat (4) 
            {
            polll = instance_create(x - 20, y - 6, o_boss_corpseblossom_pollen);
            polll.speed_x = -1 - random(2);
            polll.speed_y = -1 - random(1);
            }
        repeat (4) 
            {
            polll = instance_create(x + 10, y - 8, o_boss_corpseblossom_pollen);
            polll.speed_x = +1 + random(2);
            polll.speed_y = -1 - random(1);
            }
        spawn_able_timer = 2;
        }
    if spawn = 4 and spawn_able = true then 
        {    
        repeat (4) 
            {
            polll = instance_create(x - 6, y - 15, o_boss_corpseblossom_pollen);
            polll.speed_x = -1 - random(1);
            polll.speed_y = -1 - random(2);
            }
        repeat (4) 
            {
            polll = instance_create(x + 4, y + 2, o_boss_corpseblossom_pollen);
            polll.speed_x = +1 + random(1);
            polll.speed_y = -1 - random(2);
            }
        spawn_able_timer = 2;
        }
    }
    
// Pollen spawn //
if spawn_able_timer &gt; 0 then
    {
    // Tick down //
    spawn_able_timer -= dt();
    
    // Event //
    if spawn_able_timer &lt;= 0 then spawn_able = false; 
    }

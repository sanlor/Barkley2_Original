            <kind>1</kind>
            <string>///Create
textTitle = "PICK YOUR NUMBERS FOR THE DRAW";
textTop = textTitle;

cRed = make_color_rgb(255 - 16, 16, 16);
cYellow = make_color_rgb(255 - 16, 255 - 16, 16);
cBlue = make_color_rgb(16, 16, 255 - 16);

textWrong = "SORRY TRY AGAIN!";
textRight = "COOL BEANS";

state = 0;
numbers = 8;
numberSelect = 0;
numberDo = 0;
numberCou = 30;
for (i = 0; i &lt; numbers; i += 1)
{
    number[i] = 0;
    numberDraw[i] = 0;
}
</string>

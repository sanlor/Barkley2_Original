    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///keys
if (MiniKeyPress("cancel")) { Minigame("stop"); instance_destroy(); }

if (state == 0)
{
    if (MiniKeyPress("left"))
    {
        numberSelect -= 1;
        if (numberSelect &lt; 0) numberSelect = numbers - 1;
    }
    if (MiniKeyPress("right"))
    {
        numberSelect += 1;
        if (numberSelect &gt;= numbers) numberSelect = 0;
    }
    if (MiniKeyPress("up"))
    {
        number[numberSelect] += 1;
        if (number[numberSelect] &gt; 9) number[numberSelect] = 0;
    }
    if (MiniKeyPress("down"))
    {
        number[numberSelect] -= 1;
        if (number[numberSelect] &lt; 0) number[numberSelect] = 9;
    }
    
    if (MiniKeyPress("action"))
    {
        state = 1;
    }
}
else if (state == 1)
{
    for (i = numberDo; i &lt; numbers; i += 1) numberDraw[i] = floor(random(10));
    numberCou -= dt() * 6;
    if (numberCou &lt; 0)
    {
        numberCou += 30;
        numberDo += 1;
        if (numberDo &gt;= numbers) 
        {
            //if (keyboard_check(vk_shift)) for (i = 0; i &lt; numbers; i += 1) numberDraw[i] = 0;
            state = 2;
            right = 0;
            for (i = 0; i &lt; numbers; i += 1) if (numberDraw[i] == number[i]) right += 1;
            if (right == numbers) { audio_play_sound_ext("sn_ptonyright", 10, 0); textTop = textRight; }
            else { audio_play_sound_ext("sn_ptonywrong", 10, 0); textTop = textWrong; }
        }
    }
}
else if (state == 2)
{
    if (MiniKeyPress("action"))
    {
        //room_goto(rMiniSelect);
        instance_destroy();
        instance_create(0, 0, oMini10Select);
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>

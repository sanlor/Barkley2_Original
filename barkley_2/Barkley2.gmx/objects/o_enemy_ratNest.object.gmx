            <string>// Pause //
if paused() exit;

// Starting delay to prevent rats spawning right away after room load //
if starting_delay &gt; 0 then starting_delay -= dt();
else
    {
    // Spawn //
    if spawning = false then
        {
        // Player activates the ratnest //
        if distance_to_object(PlayerCombatActor) &lt;= 64 then
            {
            spook_eyes = 1;
            spawning = true;
            spawn = 10 + irandom(2) * 5;
            }
        }
        
    // Spawn //
    if spawn &gt; 0 then
        {
        // Tick down //
        spawn -= dt();
        
        // Event //
        if spawn &lt;= 0 then
            {
            instance_create(x, y, o_enemy_ratDummy);
            if rats &gt; 0 then 
                {
                spawning_reset = 15;
                rats -= 1;
                }
            }
        }
        
    // Reset spawning //
    if spawning_reset &gt; 0 then
        {
        // Tick down //
        spawning_reset -= dt();
        
        // Event //
        if spawning_reset &lt;= 0 then spawning = false;
        }
    }
    
// SPook eyes //
if spook_eyes &gt; 0 then
    {
    // Tick down //
    spook_eyes -= dt();
    
    // Event //
    if spook_eyes &lt;= 0 then
        {
        spook_eyes = 2;
        eyes += 1;
        visible = true;
        if eyes = 1 then image_index = 1;
        else if eyes = 2 then image_index = 2;
        else if eyes = 3 then image_index = 1;
        else 
            {
            visible = false;
            spook_eyes = 0;
            eyes = 0;
            }
        }
    }
</string>

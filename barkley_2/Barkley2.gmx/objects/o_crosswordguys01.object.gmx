if(timerfrm&lt;animInterv)
{
    timerfrm+=5*dt();
}
else
{
    timerfrm -= animInterv;
    if(animfrm&gt;0){animfrm-=7*dt();}
    else
    {
        image_index+=1;
        curImg = image_index;
        event_user(5);
    }
}

// Sound //
if _active = true then
    {
    if timer_delay &gt; 0 then
        {
        timer_delay -= dt();
        if timer_delay &lt;= 0 then 
            {
            audio_emitter_falloff(audioemit,80,150,1);
            audio_emitter_gain(audioemit,0.4 * global.sfx_gain_master);
            audio_emitter_position(audioemit,x,y,0);            
            }
        }
    else
        {
        if !audio_is_playing_ext("sn_altijd_pencil1") and irandom(100) &lt;= 10 and curImg &gt;= 7 and curImg &lt;= 17 then audio_play_sound_on_ext(audioemit,"sn_altijd_pencil1",false,80);  
        }
    } 

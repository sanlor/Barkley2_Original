o_curs.toggleCursor = true;

// Redfield - MUST be here
if (shopType == "gun")
{
    if (instance_exists(o_gunsalesman))
    {
        for (i = 0; i &lt; 5; i += 1) o_gunsalesman.gunBuy[qal, i] = itmBuy[i];
    }

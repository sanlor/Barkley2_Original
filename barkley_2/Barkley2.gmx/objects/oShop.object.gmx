DrawGunName(drx, dry, gunBot[? "pPrefix1"], gunBot[? "pPrefix2"], gunBot[? "pMaterial"], gunBot[? "pType"], gunBot[? "pSuffix"], 1);
dry += 24;

// Gunsmap
Gunsmap("draw", gunDrx + 9, gunDry + 105, 1, Gunsmap("type to index", gun[? "pType"]), gun[? "gunmap_pos"]);
//draw_sprite_ext(sCandy, 0, gunDrx + 9, gunDry + 105, 1, 1, 0, c_white, gunVss);

draw_set_alpha(1);

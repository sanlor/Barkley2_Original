                        scr_event_build_fade(true, 3);
                        }
                        
                    // Decline //
                    var no = scr_event_build_add_choice(choice_go_on, "I don't want any part in this...");
                    with no {

                        scr_event_build_dialogue(P_NAME, s_port_hoopz, "I don't think I should be here. I'm not sure what is going on between you two but... I feel like me being here would just make it harder for you guys to talk it out.");
                        scr_event_build_dialogue("Pinocchio", NULL, "I understand. It's okay, " + string(P_NAME) + ". I will deal with Geppetto on my own.");
                        scr_event_build_dialogue("Geppetto", "s_port_geppetto_beaten", "...");
                        scr_event_build_dialogue(P_NAME, s_port_hoopz, "I'll leave you guys to it then.");
                        scr_event_build_move_to_point_ext(o_cts_hoopz, 456, 210, MOVE_FAST);
                        scr_event_build_quest_state("pdt_geppetto", 1);
                        scr_event_build_fade(true, 3);
                        }

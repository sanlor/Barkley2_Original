        <arguments>
          <argument>
            <kind>1</kind>
            <string>// INHERITANCE //
event_inherited();

// PAUSE //
if paused() exit;

// APPEAR //
if timer_appear &gt; 0 then
    {
    // TICK DOWN //
    timer_appear -= dt();
    }
else
    {
    // Stay in water //
    if scr_collision_point_free(x + lengthdir_x(24, mydir), y + lengthdir_y(24, mydir), z, COLLISION_WADING) then mydir -= 6;
    else
        {
        // Slither //
        if turn = -1 then mydir -= 2.5;
        else mydir += 2.5;
        }
    
    // Speed //
    speed_x = lengthdir_x(1, mydir);
    speed_y = lengthdir_y(1, mydir);
    
    // Turn //
    if timer_turn &gt; 0 then
        {
        // Tick down //
        timer_turn -= dt();
        
        // Event //
        if timer_turn &lt;= 0 then 
            {
            turn = -turn;
            timer_turn = 4 + irandom(2);
            }        
        }
    
    // TRAIL //
    snake = instance_create(x, y, o_critter_watersnake_trail);
    snake.mydir = mydir;
    snake.image_alpha = image_alpha;
    
    // MOVE //
    x += speed_x;
    y += speed_y;
    
    // Ring //
    if timer_ring &gt; 0 then
        {
        // Tick down //
        timer_ring -= dt();
        
        // Event //
        if timer_ring &lt;= 0 then
            {
            wave = instance_create(x, y, o_hoopz_wave);
            wave.alpha = image_alpha * 0.5;
            timer_ring = 3;
            }
        }
   
    // Dive timer //
    if timer_dive &gt; 0 then
        {
        // Tick down //
        timer_dive -= dt();
        
        // Event //
        if timer_dive &lt;= 0 then dive = true;
        }
    
    // Dive //
    if dive = true then
        {
        // SLOW DOWN TO A CRAWL //
        speed_x *= 0.9;
        speed_y *= 0.9;
        
        // DISAPPEAR //
        image_alpha -= 0.05;
        
        // ADIOS //
        if image_alpha &lt;= 0 then 
            {
            x = x_origin; 
            y = y_origin;
            timer_appear = 30 + irandom(30);
            }
        }  
    else if image_alpha &lt; 1 then image_alpha += 0.05;
    }  
</string>

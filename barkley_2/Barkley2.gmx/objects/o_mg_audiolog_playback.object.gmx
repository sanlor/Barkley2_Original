        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Pause //
if paused() then exit;

// Play /
if play &gt; 0 then
    {
    // Tick down //
    play -= dt();
    
    // Event //
    if play &lt;= 0 then
        {
        // Music volume down //
        audio_sound_gain_ext(global.bgm_current_music, 0.25, 10);
        audio_play_sound_fresh(sound, 10, false);
        }
    }

// Duration //
else if duration &gt; 0 then
    {
    // Adios //
    duration -= dt();
    
    // Adios //
    if duration &lt;= 0 then 
        {
        if !audio_is_playing_ext(sound) then instance_destroy();
        else duration = 5;
        }
    }
    
// Double holster to cancel this //
if scr_input_check_pressed(KEYID_HOLSTER, PLAYER1) then skip += 4;
if skip &gt;= 6 then instance_destroy();

// Lower skip //
if skip &gt; 0 then skip -= dt();

// Image speed //
image_index += 1.1 * dt();
</string>
          </argument>
        </arguments>
      </action>

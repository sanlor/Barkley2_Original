          <argument>
            <kind>1</kind>
            <string>// Stop the sounds if skipped //
if audio_is_playing_ext(sound) then audio_stop_sound_ext(sound);

// Music volume back to normal //
audio_sound_gain_ext(global.bgm_current_music, 1, 10);

</string>

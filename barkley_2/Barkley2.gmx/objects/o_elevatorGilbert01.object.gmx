        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Step
event_inherited();
if (moving != 0)
{
    global.event = true;
    if (moving == -1) obj = o_cinema1;
    else obj = o_cinema0;
    var avg = lerp(o_cinema0.y, o_cinema1.y, 0.5);// - o_cinema1.y;
    var dst = (avg - o_cinema1.y) / 2;
    var spd = lerp(32, 128, abs(y - avg) / dst);
    y = Goto(y, obj.y, dt_sec() * spd);
    o_cts_hoopz.y = y;
    if (y == obj.y) 
    {
        SOUNDSTOP(sound);
        Destroy(oDummy);
        moving = 0;
    }
}
if (call != 0)
{
    if (call == -1) obj = o_cinema1;
    else obj = o_cinema0;
    y = Goto(y, obj.y, dt_sec() * 128);
    if (y == obj.y) 
    {
        SOUNDSTOP(sound);
        if (call == -1) Quest("gilbertElevatorDown", 0);
        if (call == 1) Quest("gilbertElevatorDown", 1);
        call = 0;
    }
}
scr_dny(32);

// Collisions
if (collisionLast != Quest("gilbertElevatorDown"))
{
    timer_semisolid = 1;
    collisionLast = Quest("gilbertElevatorDown");
}
if (timer_semisolid &gt; 0)
{
    timer_semisolid -= 1;
    if (timer_semisolid &lt;= 0)
    {
        if (Quest("gilbertElevatorDown") == 0) 
        {
            scr_collisionSystem_add_shape_semisolid(collisionBot);
            scr_collisionSystem_remove_shape_semisolid(collisionTop);
        }
        else if (Quest("gilbertElevatorDown") == 1) 
        {
            scr_collisionSystem_add_shape_semisolid(collisionTop);
            scr_collisionSystem_remove_shape_semisolid(collisionBot);
        }
        else
        {
            scr_collisionSystem_remove_shape_semisolid(collisionTop);
            scr_collisionSystem_remove_shape_semisolid(collisionBot);
        }
    }     
}
    


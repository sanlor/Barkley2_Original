            <string>/// Elevator Create
event_inherited();
//scr_event_hook_onCommon(event_cinema);
shadow_visible = false;
_interactive = false;
_disableOutline = true;
scr_actor_unsetRigid(id);
Animation("define", "default", sprite_index, 0, 1, 0);
moving = 0;
call = 0;
sound = "sn_gilbertlift01"

if (Quest("gilbertElevatorDown") == 1) y = o_cinema0.y;
else y = o_cinema1.y;
kid = instance_create(x, y, o_elevatorGilbertCaveman01);
kid.dad = id;

// Collisions
timer_semisolid = 1;
collisionLast = Quest("gilbertElevatorDown");
collisionTop = sat_make_shape_rect(o_cinema1.x, o_cinema1.y + 4, 12, 12);
collisionBot = sat_make_shape_rect(o_cinema0.x, o_cinema0.y + 4, 12, 12);
</string>

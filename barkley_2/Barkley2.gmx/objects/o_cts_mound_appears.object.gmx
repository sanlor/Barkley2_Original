            <string>// Pause //
if paused() exit;

// Thunder strikes //
if timer_thunder &gt; 0 then
    {
    // Tick down //
    timer_thunder -= dt();
    
    // Event //
    if timer_thunder &lt;= 0 then
        {
        if thunders == 0 then
            {
            disto = irandom(0);
            dir = irandom(0);
            }
        else
            {
            if disto &lt; 40 then disto += 6 + irandom(6);
            dir += 30 + irandom(30); 
            }
        var sputnik = x + lengthdir_x(disto, dir);
        var spatnik = y + lengthdir_y(disto, dir);
        thunders += 1;
        audio_play_sound_at_xy("thunder", false, 5, sputnik, spatnik);
        audio_play_sound_at_xy("splash_out", false, 5, sputnik, spatnik);
        instance_create(sputnik, spatnik, o_enemyAttack_thunderstrike);
        scr_fx_simple(sputnik, spatnik, 0, s_effect_splash_medium);
        if thunders &lt; 3 then timer_thunder = 10;  
        else timer_thunder = 2 + irandom(2);
        }
    }

// Lifespan //
if timer_lifespan &gt; 0 then timer_lifespan -= dt();
else instance_destroy();

// Weather effects gone //
if timer_weather &gt; 0 then
    {
    // Tick down //
    timer_weather -= dt();
    
    // Event //
    if timer_weather &lt;= 0 then
        {
        if instance_exists(o_effect_dust) then with o_effect_dust instance_destroy();
        if instance_exists(o_effect_swampfog) then with o_effect_swampfog instance_destroy();
        if instance_exists(o_effect_snow) then with o_effect_snow instance_destroy();
        if instance_exists(o_effect_smog) then with o_effect_smog instance_destroy();
        }
    }
</string>

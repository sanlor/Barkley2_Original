            <string>// Inheritance //
event_inherited();

// Pause //
if paused() exit;

// Who I am //
if timer_who &gt; 0 then
{
// Tick down //
timer_who -= dt();

// Event //
if timer_who &lt;= 0 then
{
// Ivanhoe //
if i_am = "ivanhoe" then
{
script = "MYSTERY | Ivanhoe the Crusader | Here lies Ivanhoe, the knight no one had heard about. May you find fame in the next world.";            
scr_entity_animation_setRange("default", 0, 1);
}
    
// Doc Jekyll //
else if i_am = "jekyll" then 
{
script = "MYSTERY | Doctor Jekyll  | - Here lies Doctor Jekyll, the kindest and most courageous dentist we've ever known. Many candy lovers did not understand your value until you were gone. Rest in plaque.";   
scr_entity_animation_setRange("default", 1, 1);
}
    
// Mr. Hyde //
else if i_am = "hyde" then
{
script = "MYSTERY | Mister Hyde | Here lies Mister Hyde. Please watch my Let's Plays on DwarfTube at n7ww.dwarftube.org/hydetopiagaming. Gamers rule. Like and subscribe.";  
scr_entity_animation_setRange("default", 2, 1);
}
    
// Pinocchios ruined grave //
else if i_am = "pino" then
{
script = "
IF pdt_pinocchios_grave == 0 | GOTO | FIRST
IF pdt_pinocchios_grave == 1 | GOTO | SECOND
IF pdt_pinocchios_grave == 2 | GOTO | SPOOK

FIRST
DIALOG | P_NAME | I can't tell what's written on this gravestone. It's all worn and vandalized. There is a image of a tree on there. Well, at least I think it's a tree... Ugh, this grave is a pigsty. There isn't even a casket in here. What a disgrace.
QUEST  | pdt_pinocchios_grave = 1
            
SECOND
DIALOG | P_NAME | Grave with an image of a tree on it. Very poorly maintained overall. If someone would ask me, I would rate this grave 3 out of 10.

SPOOK
DIALOG | P_NAME | Geez Louise... I wonder if mister Quasimodo was right... *gulp* M-maybe there really is a spook on the loose.
"
scr_entity_animation_setRange("default", 3, 1);
}
}

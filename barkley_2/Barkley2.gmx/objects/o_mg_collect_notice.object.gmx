    {
    // X motion //
    if xItem &gt; 32 then xItem -= 6;
    else xItem = 32;
    
    // Y motion //
    if yItem &gt; 32 then yItem -= 6;
    else yItem = 32;
    
    // In place //
    if xItem = 32 and yItem = 32 then 
        {
        moveItem = false;
        amount = Item("count", "Fiscian Gut's");
        }
    }
    
// Rotate Item //
if rotationItemGoal = -1 then
    {
    if rotationItem &gt; -1 then rotationItem -= 0.025;
    else rotationItemGoal = 1;
    }
else if rotationItemGoal = 1 then
    {
    if rotationItem &lt; 1 then rotationItem += 0.025;
    else rotationItemGoal = -1;
    }

            <string>// Pause //
if paused() then exit;

// Pickable? //
if !place_meeting(x, y, o_mg_organ_hubert_skin) then
    {
    pickable = true;
    }
else pickable = false;
    
// Pick up //
if instance_exists(o_mg_organ_hand) and canned = false then
    {
    if pickable = true then
        {
        if scr_input_check_pressed(KEYID_ACTION, PLAYER1) and place_meeting(x, y, o_mg_organ_hand) then
            {
            if picked = false then
                {
                sound = true;
                picked = true;
                audio_play_sound_at_xy("sn_pdt_organgrab01", false, 5, x, y);
                }
            with o_mg_organ_system bloodied_hand = true;
            depth = -10340000;
            removed_from_body = true;
            fall = false;
            speed_y = 0;
            }
        if scr_input_check_released(KEYID_ACTION, PLAYER1) and picked = true then 
            {
            picked = false;
            fall = true;
            }
        }
        
    // Picked //
    if picked = true then
        {
        x = o_mg_organ_hand.x;
        y = o_mg_organ_hand.y;
        }
    }
else if removed_from_body = true then fall = true;
    
// Fall //
if fall = true then
    {
    y += speed_y;
    if speed_y &lt; 5 then speed_y += 0.1;
    if y &lt; view_yview + 160 then y += speed_y;
    else 
        {
        if sound = true then 
            {
            if canned = true then audio_play_sound_at_xy("sn_pdt_organdrop01", false, 5, x, y);
            else audio_play_sound_at_xy("sn_pdt_organgrab01", false, 5, x, y); 
            sound = false;
            }
        y = view_yview + 160
        speed_y = 0;
        }
    }
    
// Go in the bucket //
if place_meeting(x, y, o_mg_organ_bucket) and picked = false then
    {
    if canned = false then 
        {
        canned = true;
        with o_mg_organ_system organs_got += 1; 
        }
    if x &gt; o_mg_organ_bucket.x + 2 then x -= 1;
    else if x &lt; o_mg_organ_bucket.x - 2 then x += 1;
    }
</string>

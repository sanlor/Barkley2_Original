            <string>// Hoopz jumps on the tile and it activates //
if place_meeting(x, y, o_hoopz) then
    {
    // Center hoopz on this tile //
    if centered = false then
        {
        // Remove mover //
        if instance_exists(o_mg_cloister_mover) then with o_mg_cloister_mover instance_destroy();
        
        // Stop hoopz movement //
        o_hoopz.move_y = 0;        
        o_hoopz.move_x = 0;
        
        // Slide hoopz to the center //
        if o_hoopz.x &lt; x + 15 then o_hoopz.x += 1;
        else if o_hoopz.x &gt; x + 17 then o_hoopz.x -= 1;
        else o_hoopz.x = x + 16;
        
        // Slide hoopz to the center //
        if o_hoopz.y &lt; y + 15 then o_hoopz.y += 1;
        else if o_hoopz.y &gt; y + 17 then o_hoopz.y -= 1;
        else o_hoopz.y = y + 16;
        
        // Center hoopz when he is close enough //
        if o_hoopz.x = x + 16 and o_hoopz.y = y + 16 then 
            {
            // Centered //
            centered = true;
            global.cloister_slide = true;
            
            // Start moving Hoopz //
            mover = instance_create(x, y, o_mg_cloister_mover) 
            mover.mover_x = mover_x;        
            mover.mover_y = mover_y;
            }
        }      
    }
</string>

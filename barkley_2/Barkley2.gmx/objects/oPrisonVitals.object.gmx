            <string>/// Draw
/*
VARIABLES
    prisonQuest
        0 = have not advanced the quest at all
        1 = starts when the cell doors close after the Bolthios Scene (must "restpause")
        2 = on room load after restpausing in bed, initiate 20 minutes until state = 3
        3 = Hoopz can now restpause to next hour
        4 = Hoopz has restpaused, initiate 20 minutes until state = 5
        5 = Hoopz can now restpause to next hour
        6 = Hoopz has restpaused, initiate 20 minutes until state = 7
        7 = Hoopz is at the end of his (gut) rope
        8 = you've escaped one way or another (this deactivates all collisions and is easy to check)
    prisonLiberated
        0 = prison is not liberated
        1 = heard about prison quest but haven't initiated it
        2 = initiated the prison quest
        3 = completed the prison quest
        4 = liberated the prison without taking the quest (getting arrested)
*/

if (!instance_exists(oBowelGameControlObject))
{
    exit;
}

var qst = Quest("prisonQuest");
if (qst == 0) exit;
var lib = Quest("prisonVitalsHide");
if (lib == 1) exit;
var toi = Quest("prisonUsedToilet");


// Lungs is 34x29 
drx = view_xview[0] + 4;
dry = view_yview[0] + 4;
draw_sprite_ext(s_mg_diving_oxygen, 0, drx, dry, 1, 1, 0, c_white, 1);
// Stomach
// s_mg_diving_stomach = 12
drx += 17;
dry += 29 + 17;
var sub = round((qst - 1) * 1.4);
var ind = (global.DELTA_TOTAL / 150) mod 8;
if (ind &gt; 4) ind = 4 - abs(ind - 4);
if (toi) { sub = 0; ind = 0; }
draw_sprite_ext(s_mg_diving_stomach, sub + round(ind), drx, dry, 1, 1, 0, c_white, 1);

</string>

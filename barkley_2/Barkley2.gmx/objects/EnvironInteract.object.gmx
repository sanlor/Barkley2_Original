            <string>// Inherit //
event_inherited();

// Pause //
if paused() exit;

// Pushed on contact //
if push_on_collision = true then script_execute(scr_pedestrian_push);

// Slow on contact //
var isColliding = 0;
if (slow_on_collision)
{
    //if place_meeting(x, y, PlayerCombatActor) then scr_statusEffect_applyFromScriptToCombatActor(statusEffect_sh_environSlow, o_hoopz);
    // OPTIMIZE: This may cause slow down. Make sure to keep in mind.
    var env = id;
    with (CombatActor)
    {
        if (place_meeting(x, y, env)) 
        {
            scr_statusEffect_applyFromScriptToCombatActor(statusEffect_sh_environSlow, id);
            isColliding = 0;
        }
    }
} 

// Twitch upon contact //
if shrink_on_collision = true then
    {
    // Colliding with CombatActors //
    if (isColliding || place_meeting(x, y, CombatActor)) then 
        {
        // Sound //
        if soundUttered = false then
            {
            if irandom(99) &lt;= 50 then audio_play_sound_at_xy(touchSound, false, 5, x, y);
            soundUttered = true;
            }
        
        // Scale //
        image_yscale_goal = 0.75;        
        }
    else 
        {
        image_yscale_goal = 1;
        soundUttered = false;
        }
    
    // Scale //
    image_yscale = Goto(image_yscale, image_yscale_goal, dt_sec());
    }
</string>

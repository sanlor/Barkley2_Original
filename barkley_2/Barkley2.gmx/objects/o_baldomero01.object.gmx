//TODO: how do we do time now with DSL? any different?
if (Quest("baldoEscape") == 1 || ClockTime("get") &gt;= 5 || Quest("gelasioState") == 5) 
{
    scr_event_interactive_deactivate();
    instance_destroy();
    exit;
}

        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Inherit //
event_inherited();

// PAUSE //
if paused() exit;

// Artificial lifespan //
if lifespan &gt; 0 then 
    {
    // Tick down //
    lifespan -= dt();

    // Demise //
    if lifespan &lt;= 0 then instance_destroy();
    }
    
// Slither //
if homing = false then
    {
    if x &lt; x_origin then move_x += random(0.25);
    else move_x -= random(0.25);
    
    // Fade away //
    if image_alpha &gt; 0 then image_alpha -= (0.02 + random(0.02) * dt());
    else instance_destroy();
    }
else
    {
    // Fly to Hoopz //
    if instance_exists(o_cts_hoopz) then
        {
        if o_cts_hoopz.x &lt; x then move_x -= random(0.5);
        else if o_cts_hoopz.x &gt; x then move_x += random(0.5);    
        if o_cts_hoopz.y - 20 &lt; y then move_y -= random(0.5);
        else if o_cts_hoopz.y - 20 &gt; y then move_y += random(0.5);
        }
    else
        {
        if o_hoopz.x &lt; x then move_x -= random(0.5);
        else if o_hoopz.x &gt; x then move_x += random(0.5);    
        if o_hoopz.y - 20 &lt; y then move_y -= random(0.5);
        else if o_hoopz.y - 20 &gt; y then move_y += random(0.5);
        }
       
    // Z movement // 
    if z &gt; 18 then move_z -= 0.3;
    else if z &lt; 12 then move_z += 0.3;
        
    // Limits //
    if move_x &lt; -4 then move_x = -4;
    else if move_x &gt; 4 then move_x = 4;
    if move_y &lt; -4 then move_y = -4;
    else if move_y &gt; 4 then move_y = 4;
    if move_z &lt; -4 then move_z = -4;
    else if move_z &gt; 4 then move_z = 4;
    
    // Fade away //
    if image_alpha &gt; 0 then 
        {
        if place_meeting(x, y, o_hoopz) then image_alpha -= (0.05 + random(0.04) * dt());
        else image_alpha -= 0.005;
        }
    else instance_destroy();
    }

// TRAIL //
//if (floor(x) != lsx || floor(y) != lsy)
if irandom(100) &lt;= 25 then
{
    lsx = floor(x);
    lsy = floor(y);
    trail = instance_create(x, y, o_effect_drawing_point_particle_trail);
    trail.image_blend = image_blend;
    trail.depth = depth;
    trail.y = y - z;
    trail.z = z;
}
</string>

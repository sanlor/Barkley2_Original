            <string>// Pause //
if paused() exit;

// Lifespan //
if lifespan &gt; 0 then lifespan -= 1 else instance_destroy();

// Commentary //
if breathing = true then
    {
    if !audio_is_playing_ext("sn_letsplay_breath") then audio_play_sound_at_xy("sn_letsplay_breath", false, 5, view_xview + 192, view_yview + 120);
    }
else if audio_is_playing_ext("sn_letsplay_breath") then audio_stop_sound_ext("sn_letsplay_breathing");
    
// Greetings //
if timer_greetings &gt; 0 then
    {
    // Tick down //
    timer_greetings -= dt();
    
    // Event //
    if timer_greetings &lt;= 0 then breathing = true;
    }
    
// Speech clips //
if timer_speech &gt; 0 then
    {
    // Tick down //
    timer_speech -= dt();
    
    // Event //
    if timer_speech &lt;= 0 then
        {
        timer_greetings = 55;
        timer_speech = 100 + irandom(4) * 20;
        if subscribe = false and army = false then
            {   
            audio_stop_sound_ext("sn_letsplay_breathing");
            breathing = false;         
            if irandom(100) &lt;= 50 then 
                {
                army = true;
                audio_play_sound_at_xy("sn_letsplay_banger_army", false, 5, view_xview + 192, view_yview + 120);
                }
            else 
                {
                subscribe = true;
                audio_play_sound_at_xy("sn_letsplay_subscribe", false, 5, view_xview + 192, view_yview + 120);   
                }         
            }
        else if subscribe = true and army = false then
            {
            army = true;
            audio_stop_sound_ext("sn_letsplay_breathing");
            breathing = false;
            audio_play_sound_at_xy("sn_letsplay_banger_army", false, 5, view_xview + 192, view_yview + 120);  

        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Check if Mosquitos and Players exists //
if instance_exists(o_enemy_mosquito) and instance_exists(PlayerCombatActor) then
    {
    x = PlayerCombatActor.x;
    y = PlayerCombatActor.y;
    
    // How many are near player? //
    buzz = 0;
    for (i=0; i&lt;instance_number(o_enemy_mosquito); i+=1;)
        {
        with o_enemy_mosquito
            {
            if distance_to_object(PlayerCombatActor) &lt;= 64 then other.buzz += 1;
            }
        }
    
    // Buzzing sounds //
    if buzz &gt; 0 and !audio_is_playing_ext("sn_swampmosquito01") then audio_play_sound_at_xy("sn_swampmosquito01", false, 30, x, y);
    if buzz &gt; 5 and !audio_is_playing_ext("sn_swampmosquito02") then audio_play_sound_at_xy("sn_swampmosquito02", false, 30, x, y);
    if buzz &gt; 10 and !audio_is_playing_ext("sn_swampmosquito03") then audio_play_sound_at_xy("sn_swampmosquito03", false, 30, x, y);
        
    // Buzzing volumes //
    if buzz &gt; 12 and buzz &lt;= 14 then 
        {
        audio_sound_gain_ext("sn_swampmosquito01", 0.5, 0);
        audio_sound_gain_ext("sn_swampmosquito02", 0.5, 0);
        audio_sound_gain_ext("sn_swampmosquito03", 0.5, 0);
        }
    else if buzz &gt; 9 and buzz &lt;= 11 then
        {
        audio_sound_gain_ext("sn_swampmosquito01", 0.4, 0);
        audio_sound_gain_ext("sn_swampmosquito02", 0.4, 0);
        audio_sound_gain_ext("sn_swampmosquito03", 0.4, 0);
        }
    else if buzz &gt; 6 and buzz &lt;= 8 then
        {
        audio_sound_gain_ext("sn_swampmosquito01", 0.3, 0);
        audio_sound_gain_ext("sn_swampmosquito02", 0.3, 0);
        audio_sound_gain_ext("sn_swampmosquito03", 0.3, 0);
        }
    else if buzz &gt; 3 and buzz &lt;= 5 then
        {
        audio_sound_gain_ext("sn_swampmosquito01", 0.2, 0);
        audio_sound_gain_ext("sn_swampmosquito02", 0.2, 0);
        audio_sound_gain_ext("sn_swampmosquito03", 0.2, 0);
        }
    else if buzz &gt; 0 and buzz &lt;= 2 then
        {
        audio_sound_gain_ext("sn_swampmosquito01", 0.1, 0);
        audio_sound_gain_ext("sn_swampmosquito02", 0.1, 0);
        audio_sound_gain_ext("sn_swampmosquito03", 0.1, 0);
        }
    else if buzz = 0 then
        {
        if audio_is_playing_ext("sn_swampmosquito01") audio_stop_sound_ext("sn_swampmosquito01");
        if audio_is_playing_ext("sn_swampmosquito02") audio_stop_sound_ext("sn_swampmosquito02");
        if audio_is_playing_ext("sn_swampmosquito03") audio_stop_sound_ext("sn_swampmosquito03");
        }
    }
</string>

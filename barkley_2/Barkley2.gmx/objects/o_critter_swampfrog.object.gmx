            <string>// INHERITANCE //
event_inherited();

// PAUSE //
if paused() exit;

// Gets spooked, jumps //
if distance_to_object(CombatActor) &lt;= 120 or distance_to_object(o_bullet) &lt;= 120 then 
    {
    // The jummp start //
    if jump = false then
        {
        // Only occurs once //
        jump = true;
        
        // Motion //
        move_x = random(2) * choose(1, -1);
        move_y = random(2) + 3.5;
        move_z = 7;
        gravity_z = 0.8;
        z += 2;
        
        // Facing //
        if move_x &lt; 0 then image_xscale = -1;
        }
    }
else
    {
    // Ribbit //
    if image_index &lt; 1 then image_index += 0.1;
    else image_index = 0;
    }
    
// JUMP //
if jump = true then
    {   
    // IMAGE // FLIP //
    if move_z &gt;= -1 and move_z &lt;= 1 then image_index = 3;
    
    // RISE //
    else if move_z &gt;= 1 then image_index = 2;
    
    // DIVE //
    else image_index = 4;
    
    // Hit the water //
    if z &lt;= 0 then 
        {
        // Splash effect // TODO get frankie to make a real splash effect //
        scr_fx_simple(x, y, z, s_effect_splash_medium);
        effectSimple.image_speed = dt();
        
        // Splash sound //
        audio_play_sound_at_xy("debug_one", false, 10, x, y);
        
        // Remove self //
        instance_destroy();
        }
    }
    
</string>

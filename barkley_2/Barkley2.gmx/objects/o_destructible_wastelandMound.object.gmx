            <string>// Money
var amount = 100 + irandom(200);
var billcount = 0;

while(amount&gt;0 &amp;&amp; billcount&lt;4)
{
    billcount+=1;
    var _obj = instance_create(x,y,o_money);
    _obj.move_x = random(4)-2;
    _obj.move_y = random(4)-2;
    _obj.move_z = random(2)+1;
    _obj.rot = random(360);
    _obj.scale = 1;
    _obj.rotspd = random(10)-5;
    
    var _tryam = random(amount);
    
    if(_tryam&gt;100)
    {
        _obj.money_bonus = 100;
        _obj.col = c_purple;
        _obj.scale = 1.2;
        amount-=100;
    }
    else if(_tryam&gt;50)
    {
        _obj.money_bonus = 50;
        _obj.col = c_green;
        amount-=50;
    }
    else if(_tryam&gt;25)
    {
        _obj.money_bonus = 25;
        _obj.col = c_aqua;
        amount-=25;
    }
    else if(_tryam&gt;10)
    {
        _obj.money_bonus = 10;
        _obj.col = c_teal;
        _obj.scale = 0.9;
        amount-=10;
    }
    else if(_tryam&gt;5)
    {
        _obj.money_bonus = 5;
        _obj.col = c_maroon;
        _obj.scale = 0.8;
        amount-=5;
    }
    else if(_tryam&gt;2)
    {
        _obj.money_bonus = 2;
        _obj.col = c_gray;
        _obj.scale = 0.6;
        amount-=2;
    }
    else{amount = 0;}
}

// Candy

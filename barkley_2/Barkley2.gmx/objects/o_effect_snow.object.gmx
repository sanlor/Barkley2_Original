        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Make snows
event_inherited();
if paused() exit;
if (snoWai &gt; 0)
{
    snoWai -= 1;
    if (snoWai == 0)
    {
        snoCou = snoFrq * 2;
        snoMak = 1;
        event_user(0);
        snoMak = 0;
    }
}
snoCou += dt_sec() * snoFrq;
event_user(0);

// If portal is open, make no stags //
if Quest("triskelionPortal") == 8 and room = r_ice_outskirtsB2_01 then exit;

// Create ooze stags
eneCou += dt_sec() / eneFrq;
if (eneCou &gt;= 1)
{
    eneCou -= 1;
    if (instance_number(o_enemy_jalapeno_stag) &lt; eneMax and room != r_ice_dojoOutdoors01 and room != r_ice_dojoPond01 and room != r_ice_treasure01 and scr_area_get() != "gau" and scr_area_get() != "pea") then
    {
        orx = o_hoopz.x;
        ory = o_hoopz.y;
        for (var _i = 0; _i &lt; 15; _i += 1)
        {
            var dir = random(360);
            var dis = 280;
            var linDis = scr_collision_line_distance_absolute(orx, ory, 
            orx + lengthdir_x(dis, dir), ory + lengthdir_y(dis, dir), 16, COLLISION_ALL);
            
            if (linDis == point_distance(orx, ory, orx + lengthdir_x(dis, dir), ory + lengthdir_y(dis, dir)))
            {
                crx = orx + lengthdir_x(linDis, dir);
                cry = ory + lengthdir_y(linDis, dir);
                ene = instance_create(crx, cry, o_enemy_jalapeno_stag);
                with (ene) scr_AI_aggro_configure(9999, 9999, 9999);
                break;
            }
        }
    }
}

</string>

basCol = make_color_rgb(254, 254, 254);
basColR = color_get_red(basCol);
basColG = color_get_green(basCol);
basColB = color_get_blue(basCol);
colRng = 1;
colRn2 = 2;

eneFrq = 15; // Every ten seconds make an ooze stag
eneMax = 2;
eneCou = 0;


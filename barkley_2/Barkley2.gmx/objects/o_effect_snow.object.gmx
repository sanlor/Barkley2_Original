            <string>/// Make snows
while (snoCou &gt;= 1)
{
    midX = view_xview[0] + (SCREEN_WIDTH / 2);
    midY = view_yview[0] + (SCREEN_HEIGHT / 2);
    midX = clamp(midX, (SCREEN_WIDTH / 2), room_width - (SCREEN_WIDTH / 2));
    midY = clamp(midY, (SCREEN_HEIGHT / 2), room_height - (SCREEN_HEIGHT / 2));
    midX += -(snoVarX / 2) + random(snoVarX);
    midY += -(snoVarY / 2) + random(snoVarY);
    snoLst = instance_create(midX, midY, o_effect_snowflake);
    if (snoMak) snoLst.z = snoLst.z * random(1);
    snoCou -= 1;
}

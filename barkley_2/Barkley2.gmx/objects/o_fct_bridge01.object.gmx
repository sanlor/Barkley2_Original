            <kind>1</kind>
            <string>// Inheritance //
event_inherited();

// Collision //
dx = x - sprite_get_xoffset(sprite_index);
dy = y - sprite_get_yoffset(sprite_index);
shapo = sat_make_shape_rect(dx + 16, dy + sprite_height/2, 16, sprite_height/2 + 8);

// Animation and depth //
image_speed = 0;
animspd = 0;
dny = sprite_height;

// Non-Goop room or Goop room //
if room = r_fct_accessHall01 then
    {
    if x &lt; room_width/2 then extended = true;
    else extended = false;
    }
else if room = r_fct_accessHallGooped01 then
    {
    if x &lt; room_width/2 then 
        {
        if Quest("factoryBridge") &gt;= 1 then extended = true;
        else extended = false;
        }
    else if x &gt; room_width/2 then 
        {
        if Quest("factoryBridge") &gt;= 2 then extended = true;
        else extended = false;
        }
    }
    

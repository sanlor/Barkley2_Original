            <string>// Inheritance //
event_inherited();

// Pause //
if paused() exit;

// Extended //
if extended = true then 
    {
    if y &gt; ystart then 
        {
        y -= 40 * dt_sec();
        if y &lt;= ystart then 
            {
            scr_collisionSystem_add_shape_semisolid(shapo);
            scr_collisionSystem_remove_shape_semisolid(shapo);
            y = ystart;
            }
        }
    }
else 
    {
    if y &lt; ystart + sprite_height then 
        {
        y += 40 * dt_sec();
        if y &gt;= ystart + sprite_height then 
            {
            scr_collisionSystem_add_shape_semisolid(shapo);
            y = ystart + sprite_height;
            }
        }
    }

// Delay //
if delay &gt; 0 then
    {
    // Tick down //
    delay -= 1;
    
    // Event //
    if delay &lt;= 0 then
        {
        if extended = true then 
            {
            y = ystart;
            scr_collisionSystem_add_shape_semisolid(shapo);
            scr_collisionSystem_remove_shape_semisolid(shapo);
            }
        else 
            {
            y = ystart + sprite_height;
            scr_collisionSystem_add_shape_semisolid(shapo);
            }
        }
    }

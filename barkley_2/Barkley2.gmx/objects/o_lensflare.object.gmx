            <kind>1</kind>
            <string>/// Draw

// Get time as value of percent
pct = 0.5 + lengthdir_x(-0.5, 180 * (timCur / tim)); // Smooth
cux = lx0 + ((lx2 - lx0) * pct);
cuy = ly0 + ((ly2 - ly0) * pct);
enx = lx1 - ((lx2 - lx0) * pct);
eny = ly1 - ((ly2 - ly0) * pct);

lxd = (enx - cux) / lenAmt;
lyd = (eny - cuy) / lenAmt;
drx = view_xview[0] + cux;
dry = view_yview[0] + cuy;
if (pct &lt; 0.5) alp = pct * 2; else alp = 1 - ((pct - 0.5) * 2);
alp = clamp(alp, 0, 1);
draw_set_blend_mode(bm_add);
for (i = 0; i &lt; lenAmt; i += 1)
{
    scl = (1.0 / 64) * lenSiz[i];
    sc2 = (1.0 / 64) * (lenSiz[i] - 2);
    draw_sprite_ext(s_lensflare, 0, drx, dry, scl, scl, lenRot[i], lenCol[i], alp);
    draw_sprite_ext(s_lensflare, 1, drx, dry, sc2, sc2, lenRot[i], lenCol[i], alp);
    drx += lxd;
    dry += lyd;
}
draw_set_blend_mode(bm_normal);

timCur += dt_sec();
if (timCur &gt;= tim) instance_destroy();
</string>

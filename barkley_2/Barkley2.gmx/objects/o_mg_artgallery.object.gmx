    draw_set_alpha(alpha * 0.5);
    draw_rectangle(view_xview - 2, view_yview - 2, view_xview + 386, view_yview + 242, false);
    
    // Make a textbox //
    draw_set_alpha(alpha);
    draw_rectangle(view_xview + 32, view_yview + 32, view_xview + 352, view_yview + 208, false);
    
    // Set the text //
    draw_set_font(global.fn_1);
    draw_set_color(make_color_rgb(50, 255, 255));
    draw_text(view_xview + 40, view_yview + 40, picture_name[picture_index]);
    draw_set_font(global.fn_small);
    draw_set_color(c_white);
    draw_set_alpha(alpha);
    draw_text_ext_transformed(view_xview + 40, view_yview + 64, picture_text[picture_index], -1, 304, picture_scale, picture_scale, 0);
    }

// Draw info bar //
draw_set_color(c_black);
draw_set_alpha(alpha);
draw_rectangle(view_xview - 2, view_yview + 224 + infobar_yposition, view_xview + 386, view_yview + 242 + infobar_yposition, false);
draw_set_color(c_white);
draw_set_font(global.fn_2);
draw_text(view_xview + 8, view_yview + 228 + infobar_yposition, "ACTION - TOGGLE");
draw_text(view_xview + 284, view_yview + 228 + infobar_yposition, "HOLSTER - EXIT");
draw_set_alpha(1);

</string>

        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Pause //
if paused() then exit;

// Initialize //
if initialize = true then
    {
    // Which pic? // Based on the x value of the object since variable setting hasn't been working in the EVENT system // Don't want to use questvars for this //
    picture_index = x;
    
    // Make it visible //
    visible = true;
    }

// Alpha //
if alpha &lt; alpha_goal then alpha += 0.05;
else if alpha &gt; alpha_goal then alpha -= 0.05;

// Scale //
if picture_scale &lt; picture_scale_goal then picture_scale += 0.005;
if picture_scale &gt; picture_scale_goal then picture_scale -= 0.005;

// Info bar position //
if infobar_yposition &lt; infobar_yposition_goal then infobar_yposition += 0.5;
if infobar_yposition &gt; infobar_yposition_goal then infobar_yposition -= 0.5;

// Toggle cooldown //
if timer_toggle_cooldown &gt; 0 then timer_toggle_cooldown -= dt();

// Skip cooldown //
if timer_skip_cooldown &gt; 0 then timer_skip_cooldown -= dt(); 

// Toggle //
if scr_input_check_pressed(KEYID_ACTION, PLAYER1) and timer_toggle_cooldown &lt;= 0 then
    {
    // Toggles between text description and picture //
    if toggle_picture_and_text = 0 then toggle_picture_and_text = 1;
    else toggle_picture_and_text = 0;
    }

// Skip //
if scr_input_check_pressed(KEYID_HOLSTER, PLAYER1) and timer_skip_cooldown &lt;= 0 then
    {
    // End the viewing experience //
    timer_toggle_cooldown = 50;
    timer_skip_cooldown = 50;
    alpha_goal = 0;
    picture_scale_goal = 0;
    infobar_yposition_goal = 16;
    dimmer_goal = 1;
    }
    
// Adios // Remove after fading out //
if alpha_goal = 0 and alpha = 0 then 
    {
    global.event = false;
    instance_destroy();
    }
    
// DIMMER //
scr_mg_dimmer("step");
</string>

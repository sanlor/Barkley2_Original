        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Inherit //
event_inherited();

// Pause //
if paused() exit;

// Semisolid timer //
if timer_semisolid &gt; 0 then
    {
    // Tick down //
    timer_semisolid -= dt();
    
    // Event //
    if timer_semisolid &lt;= 0 then
        {
        // Semisolid //
        if Quest("ladderReached") == 0 or Quest("govQuest") == 5 then scr_collisionSystem_add_shape_semisolid(ladderShape);
        }
    }
    
// Ladder pos //
if Quest("ladderReached") == 1 then 
    {
    ladder_position_goal = 84;
    _disableOutline = true;
    _interactive = false;
    }
    
// Ladder position //
if ladder_position &lt; ladder_position_goal then 
    {
    if ladder_speed &gt; 30 then ladder_speed -= 1;    
    ladder_position += ladder_speed * dt_sec();
    if ladder_position &gt;= 84 then ladder_position = 84;
    }
else if ladder_position = ladder_position_goal and ladder_position_goal != 16 and Quest("ladderReached") == 1 then
    {
    scr_collisionSystem_remove_shape_semisolid(ladderShape);
    Quest("ladderReached", 2);
    }

//if (o_hoopz.y &gt;= 680) event_user(0);
//else event_user(1);
</string>

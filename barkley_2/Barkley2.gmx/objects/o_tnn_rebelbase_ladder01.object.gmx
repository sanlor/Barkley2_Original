            <kind>1</kind>
            <string>// Setup //
event_inherited();
ladder_position = 16;
ladder_position_goal = 16;
timer_semisolid = 5;
ladder_speed = 72;
_interactive_distance = 40;

// Make a Shape //
ladderShape = sat_make_shape_rect(o_map_ladder.x + 16, o_map_ladder.y + 48, 32 / 2, 96 / 2);

// Has the ladder been kicked down already? //
if Quest("ladderReached") &gt;= 1 then
    {
    ladder_position = 84;
    ladder_position_goal = 84;
    _disableOutline = true;
    _interactive = false;
    }
else

            <kind>1</kind>
            <string>event_inherited();
scr_actor_unsetRigid(id);
shadow_radius = false;

// Animation states for less and less items //
scr_entity_animation_define("default", s_wilmerStash01, 0, 1, 0); // All remain //
scr_entity_animation_define("taken1", s_wilmerStash01, 1, 1, 0); // Mortgage taken //
scr_entity_animation_define("taken2", s_wilmerStash01, 2, 1, 0); // Money taken //
scr_entity_animation_define("taken3", s_wilmerStash01, 3, 1, 0); // Allowance taken //
scr_entity_animation_define("taken4", s_wilmerStash01, 4, 1, 0); // Gun taken //
scr_entity_animation_define("taken5", s_wilmerStash01, 5, 1, 0); // Candy taken //
scr_entity_animation_define("taken6", s_wilmerStash01, 6, 1, 0); // Recipe taken //

// All items have been ransacked //
if (Quest("wilmerItemsTaken")) { scr_entity_animation_set(id, "taken6"); }

scr_event_hook_onCommon(event_cinema);

script = "
IF body == governor | GOTO | GOVERNOR
IF wilmerItemsTaken == 0 | GOTO | FIRST
GOTO | SECOND

GOVERNOR
IF wilmerItemsTaken == 0 | DIALOG | Governor Elagabalus | (Mr. Wilmer's things... I should grab these later.)
IF wilmerItemsTaken == 1 | DIALOG | Governor Elagabalus | (This is where Mr. Wilmer used to keep his stuff...)

FIRST
DIALOG | P_NAME | (It's the stuff Mr. Wilmer wanted me to take. Let's see...)
USEAT  | o_wilmerStash01
SOUND  | sn_mnu_noteFlipLight01
SET    | o_wilmerStash01 | taken1
WAIT   | 0.25
NOTE   | take   | Wilmer's Amortization Schedule
NOTIFY | Got Wilmer's Amortization Schedule.
WAIT   | 0.25
DIALOG | P_NAME | (This funny-looking paper seems like Mr. Wilmer's bill.)
WAIT   | 0.25
BREAKOUT | add  | money
DIALOG | P_NAME | (There's a bunch of neuro-shekels here...)
USEAT  | o_wilmerStash01
SET    | o_wilmerStash01 | taken2
SOUND  | sn_hoopz_money2
QUEST  | money += @money_wilmerMortgage@
WAIT   | 0.25
DIALOG | P_NAME | (...@money_wilmerMortgage@ for the mortgage...)
USEAT  | o_wilmerStash01
SET    | o_wilmerStash01 | taken3
SOUND  | sn_hoopz_money3
QUEST  | money += @money_wilmerMortgageBonus@
WAIT   | 0.25
DIALOG | P_NAME | (...and @money_wilmerMortgageBonus@ for my allowance.)
BREAKOUT | clear
USEAT  | o_wilmerStash01
SET    | o_wilmerStash01 | taken4
SOUND  | sn_shotgun_reload02
WAIT   | 0.25
EVENT  | o_wilmerStash01 | 0 // You get the recipe, guns, and candies here
NOTIFY | Got Widower Wilmer's Gun (WILM)!
NOTIFY | Got the Late Esther's Gun! (ESTR)
DIALOG | P_NAME = s_port_hoopzSmirk | (I bet Mr. Wilmer brasted a lot in his day...)
DIALOG | P_NAME = s_port_hoopzSad   | (Sorry to see this about his wife though...)
WAIT   | 0.25
USEAT  | o_wilmerStash01
SET    | o_wilmerStash01 | taken5
SOUND  | sn_hoopz_candy06
WAIT   | 0.25
NOTIFY | Filled pockets with Butterscotch!
DIALOG | P_NAME | (I'll need a lot of these candies if I get hungry later...)
WAIT   | 0.25
USEAT  | o_wilmerStash01
SET    | o_wilmerStash01 | taken6
SOUND  | sn_mnu_noteFlipMedium01
WAIT   | 0.25
NOTIFY | Got Butterscotch Recipe!
DIALOG | P_NAME | (Maybe I can use this recipe somewhere to make more candies...)
WAIT   | 1
DIALOG | P_NAME | (That's everything. I think I'm good to go.)
QUEST  | wilmerItemsTaken = 1

SECOND
DIALOG | P_NAME | (Looks like I looted this table good'n'proper. Nothing left to loot.)
";
</string>

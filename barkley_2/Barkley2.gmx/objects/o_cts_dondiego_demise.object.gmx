            <string>// Pauses //
if paused() exit;

// Pauses //
if change &gt; 0 then
    {
    // Tick down //
    change -= dt();
    
    // Change subtitle //
    if change &lt;= 0 then
        {
        // Invert effect on first subtitle //
        if subtitle = 0 then 
            {
            instance_create(x, y, o_effect_dondiego_heartattack);
            scr_entity_animation_set(o_pdt_dondiego01, "invert");
            timer_painface = 10;
            }
            
        // Subtitles move on //
        subtitle += 1;
        
        // If subtitles have or haven't reached their end //
        if subtitle &lt; 4 then 
            {
            if subtitle != 0 then audio_play_sound_at_xy(audio[subtitle], false, 10, view_xview + 192, view_yview + 120);
            change = pause[subtitle];
            alpha = 3;
            }
        else 
            {
            // Blow up //
            scr_entity_animation_set(o_pdt_dondiego01, "explode");
            audio_play_sound_at_xy("sn_pdt_wilhelm01", false, 10, o_pdt_dondiego01.x, o_pdt_dondiego01.y);
            timer_end = 50;
            timer_explode = 10;
            subtitle = 0;
            }
        }
    }
    
// Start blinking //
if timer_painface &gt; 0 then
    {
    // Tick down //
    timer_painface -= dt();
    
    // Event //
    if timer_painface &lt;= 0 then scr_entity_animation_set(o_pdt_dondiego01, "pain3");
    }
    
// End //
if timer_end &gt; 0 then
    {
    // Tick down //
    timer_end -= dt();
    
    // Edges //
    if timer_end &lt;= 20 then edge -= 2;
    
    // Event //
    if timer_end &lt;= 0 then instance_destroy();
    }
else if edge &lt; 28 then edge += 2;

// Focus timer //
if timer_focus &gt; 0 then timer_focus -= dt();

// Explode //
if timer_explode &gt; 0 then
    {
    // Tick down //
    timer_explode -= dt();
    
    // Event //
    if timer_explode &lt;= 0 then
        {
        with o_pdt_dondiego01 event_user(15);
        scr_entity_animation_set(o_pdt_dondiego01, "dead");
        scr_fx_explosion_spawn_at(o_pdt_dondiego01.x, o_pdt_dondiego01.y + 50, 100, 5);
        }
    }

// Alpha //
if alpha &gt; 0 then alpha -= 0.04;
if alpha2 &lt; 1 and timer_focus &gt; 0 then alpha2 += 0.005;
else alpha2 -= 0.1;


</string>

else if (argument[0] == "from name")
{
    if (argument[1] == P_NAME_S || argument[1] == P_NAME || argument[1] == P_NAME_F) return "s_port_hoopz"
    else if (ds_map_exists(global.dsmPortrait, argument[1]) == 0) return NULL_STRING;
    else return global.dsmPortrait[? argument[1]];
}
else if (argument[0] == "define")
{
    ds_map_add(global.dsmPortrait, argument[1], argument[2]);
}
else if (argument[0] == "init")
{
    global.portraitAmount = 0;
    global.portraitMap = ds_map_create();
    _tim = get_timer();
    if (directory_exists("_portraits"))
    {
        for (file = file_find_first("_portraits\*.png", 0); file != ""; file = file_find_next())
        {
            pos = string_pos("_strip", file);
            len = string_length(file) + 1 - 4;
            if (pos > 0) { num = real(string_copy(file, pos + 6, len - (pos + 6))); global.portraitName[global.portraitAmount] = string_copy(file, 1, pos - 1); }
            else { num = 1; global.portraitName[global.portraitAmount] = string_copy(file, 1, len - 1); }
            // START Ignore end of file number, use width
            sprTmp = sprite_add("_portraits\" + file, 1, 0, 0, 0, 0);
            num = sprite_get_width(sprTmp) / 34;
            sprite_delete(sprTmp);
            // END
            global.portraitSprite[global.portraitAmount] = sprite_add("_portraits\" + file, num, 0, 0, 0, 0);
            global.portraitMap[? global.portraitName[global.portraitAmount]] = global.portraitSprite[global.portraitAmount];
            global.portraitAmount += 1;
            //show_debug_message("Portrait() - Added " + file + " as " + global.portraitName[global.portraitAmount]);
        }    
    }
    global.dsmPortrait = ds_map_create(); // Name to string_portrait
    Portrait("db");
    show_debug_message("Portrait() - All " + string(global.portraitAmount) + " portraits loaded externally in " + string((get_timer() - _tim) / 1000000) + " seconds.");
}
else // 0 = sprite_of_portrait - Returns external sprite ID of portrait that is blank
{
    if (ds_map_exists(global.portraitMap, argument[0]) == 0) return -1; //NULL
    else return global.portraitMap[? argument[0]];
    
    // How to add a portrait in the new system:
    
    // 1. Make a constant named what you what the reference to be (ie. s_port_ooze) 
    // and have it's value be the same as it's name but in string form. 
    // EG. s_port_ooze = "s_port_ooze"
    
    // 2. In a blank GM file, add your face animation, then export using GM's sprite strip exporter.
    
    // 3. Include the GM generated strip file in the Portraits include folder.
    
    // NOTE: I recommend doing portraits in a blank GM file because GM sometimes 
    // saves sprites that are deleted or renamed in the images folder. 
    // Doing it in a blank project ensures no junk files pile up in the images folder.
}

            <string>if (active) {
    // Move this with mouse //
    if global.keyinput_type == KEYINPUT_KEYBOARD_MOUSE then {
        // Move cursor via mouse //
        x = o_curs.x - USTATION_SCREEN_X;
        y = o_curs.y - USTATION_SCREEN_Y;
        
        // Click //
        if mouse_check_button(mb_left) {
            image_index = 1;
        } else {
            image_index = 0;
        }
        
        clickConfirm = mouse_check_button_pressed(mb_left);
        clickCancel  = false;
    }
    else {
        // Move the cursor via keyboard/gamepad //
        if scr_input_check_held(KEYID_UP, PLAYER1) or scr_input_check_held(KEYID_QUICKMENU_UP, PLAYER1) then {
            y -= 12 * dt();
        }
        else if scr_input_check_held(KEYID_DOWN, PLAYER1) or scr_input_check_held(KEYID_QUICKMENU_DOWN, PLAYER1) then {
            y += 12 * dt();
        }
        if scr_input_check_held(KEYID_LEFT, PLAYER1) or scr_input_check_held(KEYID_QUICKMENU_LEFT, PLAYER1) then {
            x -= 18 * dt();
        }
        else if scr_input_check_held(KEYID_RIGHT, PLAYER1) or scr_input_check_held(KEYID_QUICKMENU_RIGHT, PLAYER1) then {
            x += 18 * dt();
        }
        
        // Click //
        if scr_input_check_held(KEYID_CONFIRM, PLAYER1) {
            image_index = 1;
        } else {
            image_index = 0;
        }
        
        clickConfirm = scr_input_check_pressed(KEYID_CONFIRM, PLAYER1);
        clickCancel  = scr_input_check_pressed(KEYID_HOLSTER, PLAYER1);
    }
    
} else {
    image_index = 0;
    clickConfirm = false;
    clickCancel = false;
}

// Cap mouse coords to screen size
x = max(0, min(290, x));
y = max(0, min(214, y));

</string>

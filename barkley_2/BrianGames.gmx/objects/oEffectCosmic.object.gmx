            <string>///Step

//Text test
if (keyboard_check(ord('1')))
{
    keyboard_clear(ord('1'));
    if (instance_exists(o_cosmic_cood) == 0) { instance_create(211, 112, o_cosmic_cood); }
    else { with (o_cosmic_cood) instance_destroy(); }
}

//Gravity well
if (keyboard_check(ord('2')))
{
    keyboard_clear(ord('2'));
    obj = instance_create(mouse_x, mouse_y, oCosmicGravityWell);
}

//Posh - NOT TO BE CONFUSED WITH PUSH
if (keyboard_check(ord('3')))
{
    keyboard_clear(ord('3'));
    //show_debug_message("HELLO");
    obj = instance_create(211, 112, oCosmicPush); //NOT TO BE CONFUSED WITH PUSH
    obj.dir = point_direction(211, 112, mouse_x, mouse_y);
    with (obj) event_user(1);
}

//GRID
if (keyboard_check(ord('4')))
{
    keyboard_clear(ord('4'));
    if (instance_exists(oCosmicGrid) == 0) instance_create(211, 112, oCosmicGrid);
    else with (oCosmicGrid) kill = 1; //instance_destroy();
}
else
{
    
}

//BLINK
if (keyboard_check(ord('5')))
{
    keyboard_clear(ord('5'));
    global.did = 0;
    while (true)
    {
        with (o_enemy_test)
        {
            if (random(100) &lt; 33) 
            { 
                obj = instance_create(x, y - 16, oCosmicBlink); 
                obj.dad = id;
                global.did = 1; 
                break; 
            }
        }
        if (global.did == 1) break;
    }
}

//Swap blink
if (keyboard_check(ord('6')))
{
    keyboard_clear(ord('6'));
    global.did = -999;
    while (true)
    {
        with (o_enemy_test)
        {
            if (random(100) &lt; 33) 
            { 
                global.did = id; 
                break; 
            }
        }
        if (global.did != -999) break;
    }
    
    global.tObj = global.did;
    
    global.did = -999;
    while (true)
    {
        with (o_enemy_test)
        {
            if (random(100) &lt; 33 &amp;&amp; global.tObj != id) 
            { 
                global.did = id; 
                break; 
            }
        }
        if (global.did != -999) break;
    }
    
    obj = instance_create(global.tObj.x, global.tObj.y - 16, oCosmicBlink);
    obj.dad = global.tObj;
    obj.dad2 = global.did;
    obj.blink = 2;
}

//ALL BLINK
if (keyboard_check(ord('7')))
{
    keyboard_clear(ord('7'));

    with (o_enemy_test)
    {
        obj = instance_create(x, y - 16, oCosmicBlink); 
        obj.dad = id;
    }

}

//dreamDir = 90;
if (keyboard_check(ord('8')))
{
    o_hoopz_hilight.image_blend = c_orange;
    o_hoopz_hilight.image_alpha = 0.2 + random(0.1);
    repeat (3)
    {
        dreamDir -= 4;
        obj = instance_create(211, 112, oCosmicBall); 
        obj.dir = dreamDir;
    }

}
else
{
    dreamDir = 270;
}

//Flash blink
if (keyboard_check(ord('9')))
{
    oEffectCosmic.flashCol = global.cosmicColor;
}
if (keyboard_check(ord('0')))
{
    oEffectCosmic.flashCol = c_black;
}
//else oEffectCosmic.flashCol = c_black;








exit;

//Gravity well
if (keyboard_check(ord('3')))
{
    keyboard_clear(ord('3'));
    obj = instance_create(211, 112, oCosmicPush);
}

//Brainowaves
if (keyboard_check(ord('1')))
{
    if (instance_exists(o_magnet_test) == 0) instance_create(211, 112, o_magnet_test);
}
else
{
    with (o_magnet_test) instance_destroy();
}

//Brainowaves
if (keyboard_check(ord('2')))
{
    if (instance_exists(o_magnet_test2) == 0) instance_create(211, 112, o_magnet_test2);
}
else
{
    with (o_magnet_test2) instance_destroy();
}

//Laser
if (keyboard_check(ord('3')))
{
    if (instance_exists(o_laser_test) == 0) instance_create(211, 112, o_laser_test);
}
else
{
    with (o_laser_test) instance_destroy();
}

//Laser chain
if (keyboard_check(ord('4')))
{
    keyboard_clear(ord('4'));
    obj = instance_create(211, 112, o_chain_test);
    
    //get chain of people
    global.tObj = id;
    lis = ds_list_create();
    with (o_enemy_test)
    {
        ds_list_add(global.tObj.lis, id);
    }
    ds_list_shuffle(lis);
    obj.list = lis;
}

//EMP
if (keyboard_check(ord('5')))
{
    keyboard_clear(ord('5'));
    //with (o_emp_draw) instance_destroy();
    //obj = instance_create(211, 112, o_emp_draw);
    
    with (o_emp_test2) instance_destroy();
    obj = instance_create(211, 112, o_emp_test2);
    with (o_enemy_test)
        {
            obj = instance_create(x, y - 14, o_overclock_test); 
            obj.couSpd = 0.5;
            shake = 4;
        }
}

//Scan test
if (keyboard_check(ord('6')))
{
    keyboard_clear(ord('6'));
    
    global.did = 0;
    while (true)
    {
        with (o_enemy_test)
        {
            if (random(100) &lt; 33) 
            { 
            obj = instance_create(x, y - 14, o_overclock_test); 
            
            shake = 4;
            global.did = 1; 
            break; 
            }
        }
        if (global.did == 1) break;
    }
    //obj = instance_create(mouse_x, mouse_y, o_scan_test2);
}

//Cyber barrier
if (keyboard_check(ord('7')))
{
    keyboard_clear(ord('7'));
    
    with (o_barrier_test) instance_destroy();
    obj = instance_create(211, 112, o_barrier_test);
}

//Binary shower
if (keyboard_check(ord('8')))
{
    keyboard_clear(ord('8'));
    
    global.did = 0;
    while (true)
    {
        with (o_enemy_test)
        {
            if (random(100) &lt; 33) 
            { 
            obj = instance_create(x, y - 32, o_binary_shower); 
            
            //shake = 4;
            global.did = 1; 
            break; 
            }
        }
        if (global.did == 1) break;
    }
}

//cosmic test
if (keyboard_check(ord('9')))
{
    keyboard_clear(ord('9'));
    
    obj = instance_create(211, 112, o_cosmic_test);
}

//zuck
if (keyboard_check(ord('0')))
{
    keyboard_clear(ord('0'));
    //with (o_emp_draw) instance_destroy();
    //obj = instance_create(211, 112, o_emp_draw);
    
    //with (o_emp_test2) instance_destroy();
    obj = instance_create(211, 112, o_zuck_test);
}










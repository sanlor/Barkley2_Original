            <kind>1</kind>
            <string>///Draw
dir += 5.0;
event_user(1);
if (scl &lt;= 1) 
{
    alp = 1;
    xstart = (210 * abs(scl - 1)) + (xst * abs(scl - 0));
    ystart = (98 * abs(scl - 1)) + (yst * abs(scl - 0));
}
else 
{
    xstart = xst;
    ystart = yst;
    alp = abs(2 - (scl - 1)) / 2;
}

//Color
if (scl &lt;= 1) col = c_fuchsia;
else { col = merge_color(c_fuchsia, c_black, (scl - 1) / 2); }

draw_set_color(col);
draw_set_alpha((0.33 + random(0.22)) * alp);
dx = xstart - shake + random(shake * 2);
dy = ystart - shake + random(shake * 2);
draw_primitive_begin(pr_trianglefan);
draw_vertex_colour(dx, dy, col, 0);
for (i = 0; i &lt; points; i += 1)
{
    draw_vertex(dx + (px[i] * scl), dy + (py[i] * scl));
}
draw_vertex(dx + (px[0] * scl), dy + (py[0] * scl));
draw_primitive_end();
draw_set_alpha(1);

scl += (1 / 30);
if (shake &lt; 5) scl = min(scl, 1);
else { dir -= 10.0; scl += 0.1; scl = min(scl, 3); }

cou += 1;

if (cou &gt; 60)
{
    if (shake == 5)
    {
        dad.shake = 5;
        global.t0 = points;
        with (dad) 
        {
            obj = instance_create(x, y - 16, o_entity_indicatorDamage_popcorn);
            obj.number += obj.number * ((global.t0 - 2) * 50);
        }
    }
    dir -= 5.0;
    shake += 0.25;
}

if (scl == 3) instance_destroy();
</string>

            <string>///generate emp then fade
alp = 0;
cou = 0;

//do tree

joints = 0;
dirRng = 360 / 8;//45;
dir = random(360);
dis = 24;
stage = 2;
wid = 5;
jointDo = 8;
for (i = 0; i &lt; jointDo; i += 1) { jointX[i] = 0; jointY[i] = 0; jointW[i] = wid; jointD[i] = (360 / jointDo) * i; }
in = jointDo;
while (jointDo &gt; 0)
{
    if (stage &gt;= 6) exit;
    jointDo2 = 0;
    for (i = 0; i &lt; jointDo; i += 1)
    {
        dr = jointD[joints] - (dirRng / 2) + random(dirRng);
        jointDir[joints] = dr;
        jointDis[joints] = dis;
        jointX1[joints] = jointX[joints] + lengthdir_x(dis, dr);
        jointY1[joints] = jointY[joints] + lengthdir_y(dis, dr);
        for (h = 0; h &lt; stage; h += 1)
        {
            //in = joints + jointDo2 + h; //jointDo + joints + h;
            jointD[in] = dr;
            jointX[in] = jointX1[joints];
            jointY[in] = jointY1[joints];
            jointW[in] = wid - 1;
            jointDo2 += 1;
            in += 1;
        }
        joints += 1;
    }
    jointDo = jointDo2;
    stage += 1;
    wid -= 1;
    //dirRng *= 0.8;
    //dis *= 0.9;
    dis *= 1.2;
    wid -= 1;
}

o_hoopz_hilight.image_blend = c_aqua;
o_hoopz_hilight.image_alpha = 0.5;
</string>

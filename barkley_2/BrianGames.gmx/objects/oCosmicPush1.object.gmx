{
    if (i &lt; 2)
    {
        xs[i] = px * (siz + random(siz / 2));
        ys[i] = px * (siz + random(siz / 2));
        col[i] = global.cosmicColor;
    }
    else
    {
        xs[i] = px * ((siz / 3) + random(siz / 4));
        ys[i] = px * ((siz / 3) + random(siz / 4));
        col[i] = merge_color(global.cosmicColor, colorDark, .5);
    }
}

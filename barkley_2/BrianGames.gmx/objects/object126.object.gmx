    <event eventtype="7" enumb="11">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///MENU - Level Up
cTint = c_lime;
tint = 0.25;//0.25;
cHoopz = merge_color(c_lime, c_white, 1); //hoopz
alphaHoopz = 0.5;
alphaBG = 0.75;

tint = 0.25;
cText = merge_color(c_white, cTint, tint);
cTextBG = merge_color(make_color_rgb(96, 96, 96), cTint, tint);
cTextActive = merge_color(make_color_rgb(252, 198, 19), cTint, tint);
cTextSelected = merge_color(make_color_rgb(252, 250, 125), cTint, tint);
cTextInactive = merge_color(make_color_rgb(32, 32, 32), cTint, tint); //42, 46, 40

tint = 0.25;

cBGActive = merge_color(make_color_rgb(64, 64, 64), cTint, tint); //42, 46, 40
cBGInactive = merge_color(make_color_rgb(0, 0, 0), cTint, tint); //was 24

//42, 45, 40

heightTop = 16;

spc = 8;

dx = 15; dy = 16; //was 24
dxo = dx;
dyo = dy;
dw = 96; dh = heightTop;//40;
draw_set_color(cBGInactive);
draw_set_alpha(alphaBG);
draw_rectangle(dx, dy, dx + dw, dy + dh, 0);
draw_set_alpha(1);
scr_font(global.fn_1, cTextBG, 1, 1);
draw_text(dx + (dw / 2), dy + (dh / 2) + 1, "Bio Augment");

//Hoopz box
dy += dh + spc;
dh = (24 * 5) + (spc * 4);
draw_set_color(cBGInactive);
draw_set_alpha(alphaBG);
draw_rectangle(dx, dy, dx + dw, dy + dh, 0);
draw_set_alpha(1);
//scr_font(global.fn_1, c_white, 1, 1);
draw_set_colour_write_enable(0, 1, 0, 1);
draw_sprite_ext(sMenuHoopzLegs, hoopzSub, dx + (dw / 2), dy + (dh / 2), 2, 2, 0, cHoopz, alphaHoopz);
draw_sprite_ext(sMenuHoopzTorso, hoopzSub, dx + (dw / 2), dy + (dh / 2), 2, 2, 0, cHoopz, alphaHoopz);
draw_set_colour_write_enable(1, 1, 1, 1);
hoopzSub += dt() / 5;

//Cancel
dy += dh + spc;
dh = 24;
draw_set_color(cBGActive);
draw_set_alpha(alphaBG);
draw_rectangle(dx, dy, dx + dw, dy + dh, 0);
draw_set_alpha(1);
if (scr_mouse_rect(dx, dy, dx + dw, dy + dh)) { hover = 1; col = cTextActive; } else { hover = 0; col = cText; }
scr_font(global.fn_1, col, 1, 1);
draw_text(dx + (dw / 2), dy + (dh / 2), "Cancel");
if (hover == 1)
{
    if (mouse_check_button(mb_left))
    {
        mouse_clear(mb_left);
        levelPoint = 3;
        for (i = 0; i &lt; 5; i += 1) levelAmount[i] = 0;
    }
}

//Confirm
dx += dw + spc;
dw = 64 + 8 + spc + (16 * 3) + (spc * 2) + spc + 32 - 8;
draw_set_color(cBGActive);
draw_set_alpha(alphaBG);
draw_rectangle(dx, dy, dx + dw, dy + dh, 0);
draw_set_alpha(1);
if (scr_mouse_rect(dx, dy, dx + dw, dy + dh)) { hover = 1; col = cTextActive; } else { hover = 0; col = cText; }
scr_font(global.fn_1, col, 1, 1);
draw_text(dx + (dw / 2), dy + (dh / 2), "Confirm");
if (hover == 1)
{
    if (mouse_check_button(mb_left))
    {
        mouse_clear(mb_left);
        levelPoint = 3;
        for (i = 0; i &lt; 5; i += 1) 
        {
            Stat(string_lower(levelText[1, i + 1]), "+" + string(levelAmount[i]));
            levelAmount[i] = 0;
        }
    }
}
dw = 96;
//levelAmount[i - 1] + Stat(string_lower(levelText[1, i])

//BANDWIDTH and stat names
dx = dxo;
dx += dw + spc; dy = dyo;
dw = 64 + 8; dh = heightTop;
for (i = 0; i &lt; 6; i += 1)
{
    draw_set_color(cBGInactive);
    draw_set_alpha(alphaBG);
    draw_rectangle(dx, dy, dx + dw, dy + dh, 0);
    draw_set_alpha(1);
    if (i == 0) col = cTextBG;
    else 
    {
        col = cTextInactive;
        if (levelAmount[i - 1] &gt; 0) col = cTextBG;
    }
    scr_font(global.fn_1, col, 1, 1);
    
    if (i != 0)
    {
        scr_font(global.fn_2c, col, 1, 1);
        draw_text(dx + (dw / 2), dy + (dh / 2) - 6, levelText[1, i]); //-3
        if (levelAmount[i - 1] &gt; 0) col = cTextSelected;
        else col = cTextBG;
        scr_font(global.fn_1, col, 1, 1);
        draw_text(dx + (dw / 2), dy + (dh / 2) + 6, string(levelAmount[i - 1] + Stat(string_lower(levelText[1, i])))); //+8
    }
    else draw_text(dx + (dw / 2), dy + (dh / 2) + 1, levelText[1, i]);
    scr_font(global.fn_1, col, 1, 1);
    
    dy += dh + spc;
    dh = 24;
}

dx += dw + spc; dy = dyo;
dw = 16; dh = heightTop;
draw_set_color(cBGInactive);
draw_set_alpha(alphaBG);
draw_rectangle(dx, dy, dx + dw, dy + dh, 0);
draw_set_alpha(1);
if (levelPoint == 3) col = cTextActive;
else if (levelPoint &lt; 3) col = cTextSelected;//BG;
else col = cTextInactive;
scr_font(global.fn_1, col, 1, 1);
draw_text(dx + (dw / 2), dy + (dh / 2) + 1, "3");

//Actual things to click for levels
clicked = -1;
hover = 0;
dw = (16 * 3) + (spc * 2);
dy += dh + spc;
dh = 24;
for (i = 1; i &lt; 6; i += 1)
{
    col = cBGActive;
    if (levelPoint == 0) col = cBGInactive;
    else if (levelAmount[i - 1] != 0) { col = cBGInactive; }
    else if (scr_mouse_rect(dx, dy, dx + dw, dy + dh))
    {
        hover = i;
    }
    else
    {
        
    }
    
    draw_set_color(col); 
    draw_set_alpha(alphaBG);
    draw_rectangle(dx, dy, dx + dw, dy + dh, 0);
    draw_set_alpha(1);
    
    if (levelAmount[i - 1] != 0) col = cTextSelected;//Inactive;
    else if (levelPoint == 0) col = cTextInactive;
    else if (hover == i) col = cTextActive;
    else col = cText;
    
    scr_font(global.fn_1, col, 1, 1);
    //draw_text(dx + (dw / 2), dy + (dh / 2), ".");
    draw_set_color(col);
    
    saws = max(3, 3 + (levelAmount[i - 1] * 3));
    draw_sawtooth(dx, dy + (dh / 2) - 1, 0, dw - 1, levelAmount[i - 1] * 2, saws);
    draw_sawtooth(dx, dy + (dh / 2), 0, dw - 1, levelAmount[i - 1] * 2, saws);
    draw_sawtooth(dx, dy + (dh / 2) + 1, 0, dw - 1, levelAmount[i - 1] * 2, saws);
    //if (scr_mouse_rect(dx, dy, dx + dw, dy + dh)) 
    dy += dh + spc;
}
dw = 16;

if (hover != 0 &amp;&amp; levelPoint &gt; 0)
{
    if (mouse_check_button(mb_left))
    {
        mouse_clear(mb_left);
        levelAmount[hover - 1] = levelPoint;
        levelPoint -= 1;
        if (levelPoint == 0) event_user(10);
    }
}

dx += dw + spc; dy = dyo;
dw = 16; dh = heightTop;
draw_set_color(cBGInactive);
draw_set_alpha(alphaBG);
draw_rectangle(dx, dy, dx + dw, dy + dh, 0);
draw_set_alpha(1);
if (levelPoint == 2) col = cTextActive;
else if (levelPoint &lt; 2) col = cTextSelected;//BG;
else col = cTextInactive;
scr_font(global.fn_1, col, 1, 1);
draw_text(dx + (dw / 2), dy + (dh / 2) + 1, "2");

dx += dw + spc;
dw = 16; dh = heightTop;
draw_set_color(cBGInactive);
draw_set_alpha(alphaBG);
draw_rectangle(dx, dy, dx + dw, dy + dh, 0);
draw_set_alpha(1);
if (levelPoint == 1) col = cTextActive;
else if (levelPoint &lt; 1) col = cTextSelected;//BG;
else col = cTextInactive;
scr_font(global.fn_1, col, 1, 1);
draw_text(dx + (dw / 2), dy + (dh / 2) + 1, "1");

//Overclock
dx += dw + spc;
dw = 32 - 8; dh = heightTop;
hover = 0;
clicked = 0;
for (i = 0; i &lt; 6; i += 1)
{
    if (i != 0)
    {
        if (scr_mouse_rect(dx, dy, dx + dw, dy + dh) &amp;&amp; levelPoint == 2 &amp;&amp; levelAmount[i - 1] == 3) hover = i;
        if (levelPoint == 2 &amp;&amp; levelAmount[i - 1] == 3) col = cBGActive;
        else col = cBGInactive;
        draw_set_color(col);
        draw_set_alpha(alphaBG);
        draw_rectangle(dx, dy, dx + dw, dy + dh, 0);
        draw_set_alpha(1);
        if (levelAmount[i - 1] &gt;= 5) col = cTextSelected;
        else if (hover == i) col = cTextActive;
        else if (levelPoint == 2 &amp;&amp; levelAmount[i - 1] == 3) col = cText;
        else col = cTextInactive;
        scr_font(global.fn_1, col, 1, 1);
        draw_text(dx + (dw / 2), dy + (dh / 2), "OC");
    }
    
    dy += dh + spc;
    dh = 24;
}
if (hover != 0 &amp;&amp; levelPoint == 2)
{
    if (mouse_check_button(mb_left))
    {
        mouse_clear(mb_left);
        levelAmount[hover - 1] += 2;
        levelPoint -= 2;
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>

        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Create
fall = 0;
state = 0;
sub = 0;
fade = 0;
cou = 0;
col = make_color_rgb(140, 180, 200);
scr_deltatime_init();

chains = 24;
for (i = 0; i &lt; chains; i += 1)
{
    chainY[i] = y - 36;
    if (i &lt; chains / 2)
    {
        chainX[i] = x - 7;
    }
    else
    {
        chainX[i] = x + 6;
    }
    chainX[i] += -2 + floor(random(5));
    chainY[i] += -1 + floor(random(3));
    chainDir[i] = 180 - 40 + random(80);
    chainSpd[i] = (0.7 + random(0.2)) * choose(1, -1);
    chainAlp[i] = 1;
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="1">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Begin
scr_deltatime_update();

if (state == 1)
{
    //sprite_index = sTreasureDwarfFree;
    fade += dt() / 10;
    fade = clamp(fade, 0, 1);
    if (fade == 1)
    {
        state = 2;
        cou = 0;
    }
}
else if (state == 2)
{
    cou += dt();
    if (cou &gt; 5) { cou = 0; state = 3; audio_play_sound(mTreasureBreak, 10, 0); }
}
else if (state == 3)
{
    sprite_index = sTreasureDwarfFree;
    image_single = 0;
    cou += dt();
    if (cou &gt; 15) { cou = 0; state = 4; }
}
else if (state == 4)
{
    sub += dt() / 3;
    if (sub &gt;= 2) { if (fall == 0) audio_play_sound(mTreasureFall, 10, 0); fall = 1; }
    if (sub &gt;= 4) { sub = 3; state = 5; }
    image_single = floor(sub);
}
else if (state == 5)
{
    cou += dt();
    if (cou &gt; 7.5) { cou = 0; state = 6; audio_play_sound(mTreasureThanks, 10, 0); } //2
}
else if (state == 6)
{
    cou += dt();
    if (cou &lt; 3 + 1) image_single = 4;
    else if (cou &lt; 6 + 2) image_single = 5;
    else if (cou &lt; 9 + 3) image_single = 4;
    else if (cou &gt; 15 + 4) 
    { 
        y += 7; 
        sprite_index = sTreasureDwarfFlee; 
        image_single = 0; 
        state = 7; 
        sub = 0; 
        if (spd &gt; 0) image_xscale = -1;
    }
    //else if (cou &lt; 8) image_single = 3;
    //else if (cou &lt; 10) image_single = 4;
}
else if (state == 7)
{
    sub += dt() / 2;
    image_single = floor(sub);
    
    x += (dt() * 6) * spd;
    if (x &lt; view_xview[0] - 64 || x &gt; view_yview[0] + 384 + 64) { state = 8; }
}
else if (state == 8)
{
    fade -= dt() / 10;
    fade = clamp(fade, 0, 1);
    if (fade == 0)
    {
        instance_destroy();
    }
}
</string>
          </argument>
        </arguments>
      </action>

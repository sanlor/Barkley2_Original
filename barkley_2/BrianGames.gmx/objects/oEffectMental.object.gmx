        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Step

//Brainowaves
if (keyboard_check(ord('1')))
{
    if (instance_exists(o_sine_test) == 0) instance_create(208, 96, o_sine_test);
}
else
{
    with (o_sine_test) instance_destroy();
}

//Radio Junk
if (keyboard_check(ord('2')))
{
    
    arcCou -= 1;
    o_hoopz_hilight.image_alpha = (0.33 / 4) * abs(arcCou - 4);
    if (arcCou &lt;= 0)
    {
        //o_hoopz_hilight.image_alpha = 0;
        instance_create(213, 112 - 8, o_radio_test);
        arcCou = 5;
    }
}
else
{
    arcCou = 0;
}

//Shape Chaos
if (keyboard_check(ord('3')))
{
    keyboard_clear(ord('3'));
    global.points = 3;
    with (o_enemy_test)
    {
        obj = instance_create(x, y - 16, o_shape_crush);
        with (obj) { points = global.points; event_user(1); }
        obj.dad = id;
        global.points += 1;
    }
}

//Mental Barrier
if (keyboard_check(ord('4')))
{
    keyboard_clear(ord('4'));

    if (true)
    {
        obj = instance_create(211, 112, o_circle_line); //mouse_x, mouse_y, o_circle_line);
    }
    else
    {
        dx = 211;
        dy = 112;
        i0 = 48 + 16;
        i1 = 24 + 8;
        instance_create(dx, dy, o_circle_line);
        instance_create(dx + i0, dy, o_circle_line);
        instance_create(dx - i0, dy, o_circle_line);
        instance_create(dx, dy + i0, o_circle_line);
        instance_create(dx, dy - i0, o_circle_line);
        
        instance_create(dx + i1, dy + i1, o_circle_line);
        instance_create(dx - i1, dy - i1, o_circle_line);
        instance_create(dx - i1, dy + i1, o_circle_line);
        instance_create(dx + i1, dy - i1, o_circle_line);
    }
}

//Saw test
if (keyboard_check(ord('5')))
{
    keyboard_clear(ord('5'));
    
    prt = 8;
    dv = 360 / prt;
    alt = 0;
    dir = random(360);
    for (i = 0; i &lt; prt; i += 1)
    {
        obj = instance_create(210, 97, o_saw_test2);
        obj.direction = dir; //i * dv;
        if (alt == 0) obj.ampDir[0] = 90;
        alt = !alt;
        dir += dv;
    }
    
    with (o_enemy_test)
    {
        instance_create(x, y - 14, o_snare_test);
    }
    //if (instance_exists(o_saw_test) == 0) instance_create(208, 96, o_saw_test);
}
else
{
    //with (o_saw_test) instance_destroy();
}

//Scan test
if (keyboard_check(ord('6')))
{
    keyboard_clear(ord('6'));
    
    global.did = 0;
    while (true)
    {
        with (o_enemy_test)
        {
            //if (random(100) &lt; 33) 
            { 
            obj = instance_create(x - 16, y - 32, o_scan_test); 
            obj = instance_create(x - 16, y - 32, o_scan_test2);
            global.did = 1; 
            //break; 
            }
        }
        if (global.did == 1) break;
    }
    //obj = instance_create(mouse_x, mouse_y, o_scan_test2);
}

//Egg buster / echochamber
if (keyboard_check(ord('7')))
{
    keyboard_clear(ord('7'));
    instance_create(210, 98, o_eggbuster);
}

//Square lightning
if (keyboard_check(ord('8')))
{
    keyboard_clear(ord('8'));
    
    global.did = 0;
    while (true)
    {
        with (o_enemy_test)
        {
            if (random(100) &lt; 33) 
            { 
            //obj = instance_create(x, y - 24, o_square_test); 
            obj = instance_create(210, 98, o_square_scan);
            obj.dad = id;
            obj.dir = point_direction(210, 98, x, y);
            obj.len = point_distance(210, 98, x, y);
            global.did = 1; 
            break; 
            }
        }
        if (global.did == 1) break;
    }
    //obj = instance_create(mouse_x, mouse_y, o_scan_test2);
}
</string>

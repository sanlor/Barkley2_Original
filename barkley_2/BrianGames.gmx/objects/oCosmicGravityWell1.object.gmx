        <arguments>
          <argument>
            <kind>1</kind>
            <string>///particle that changes size based on proximity
ox = x;
oy = y;
len = 64 + random(16);
dir = random(360);
x = x + lengthdir_x(len, dir);
y = y + lengthdir_y(len, dir);
sx = x;
sy = y;
spd = 20;
xd = abs(ox - x);
yd = abs(oy - y);
pd = point_distance(ox, oy, x, y);
dir = point_direction(x, y, ox, oy);
px = (1.0 / 128);
alp = 0;
prt = 6;
siz = 8 + random(8);
for (i = 0; i &lt; prt; i += 1)
{
    if (i &lt; 2)
    {
        xs[i] = px * (siz + random(siz / 2));
        ys[i] = px * (siz + random(siz / 2));
        col[i] = global.cosmicColor;
    }
    else
    {
        xs[i] = px * ((siz / 3) + random(siz / 4));
        ys[i] = px * ((siz / 3) + random(siz / 4));
        col[i] = merge_color(global.cosmicColor, c_black, .5);
    }
}
</string>

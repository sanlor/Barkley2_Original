        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Double the layers of beam and only half the laser
lpd = 2;
dif = angle_difference(dir, point_direction(x, y, mouse_x, mouse_y));
dif2 = dif;
dif = clamp(dif, -lpd, lpd);
//make momentum

if (dif != 0) laser -= abs(dif) / 33; else laser += 0.05;
laser = clamp(laser, 0, 1);

if (abs(dif2) &lt;= lpd) dir = point_direction(x, y, mouse_x, mouse_y); else dir -= dif;

len = 320;
global.tx0 = x;
global.ty0 = y;
global.tx1 = x + lengthdir_x(320, dir);
global.ty1 = y + lengthdir_y(320, dir);
//in real version need to do closest enemies first
obj = collision_line(global.tx0, global.ty0, global.tx1, global.ty1, o_enemy_test, 0, 0);
if (obj != noone)
{
    len = point_distance(x, y, obj.x, obj.y);
    obj.shake = 3;
}

//Over hoopz for effect
draw_set_blend_mode(bm_add);
for (i = 0; i &lt; 4; i += 1)
{
    dr = random(360);
    dx = x + lengthdir_x(12, dr);
    dy = y + lengthdir_y(12, dr);
    col = merge_color(c_aqua, c_black, 0.9 - random(0.1) + (abs(laser - 1) / 10)); //(i * 0.13));
    sl = 0.2 + random(0.1);
    draw_sprite_ext(s_sine64, 0, dx, dy, sl, sl, 0, col, 1);
}


for (i = 0; i &lt; 8; i += 1)
{
    col = merge_color(c_aqua, c_black, 0.9 - random(0.1) + (abs(laser - 1) / 10)); //(i * 0.13));
    draw_set_color(col);
    rad = (i + 1) * 2;
    draw_circle(x, y, rad, 0);
    xl = x + lengthdir_x(len, dir);
    yl = y + lengthdir_y(len, dir);
    if (i &lt; 5) draw_line_width(x, y, xl, yl, rad * 1.5);
}

//Over enemy
if (obj != noone)
{
    for (i = 0; i &lt; 12; i += 1)
    {
        dr = random(360);
        dx = obj.x + lengthdir_x(10, dr);
        dy = obj.y - 12 + lengthdir_y(14, dr);
        col = merge_color(c_aqua, c_black, 0.8 - random(0.1) + (abs(laser - 1) / 10)); //(i * 0.13));
        sl = 0.4 + random(0.1);
        draw_sprite_ext(s_sine64, 0, dx, dy, sl, sl, 0, col, 1);
        //above was draw_get_color()
    }
}
draw_set_blend_mode(bm_normal);

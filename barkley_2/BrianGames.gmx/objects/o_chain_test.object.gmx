          <argument>
            <kind>1</kind>
            <string>///draw
draw_set_blend_mode(bm_add);
oEffectCyber.flashAlp = 0;
if (state == 0)
{
    toObj = ds_list_find_value(list, ind);
    if (cou == 0)
    {
        //flash lags behind
        oEffectCyber.flashCol = make_color_rgb(c_aqua, c_black, 0.99);
        oEffectCyber.flashAlp = 0.1;
        
        toObj.shake = 4;
        instance_create(toObj.x, toObj.y, o_entity_indicatorDamage_popcorn);
        instance_create(toObj.x, toObj.y - 14, o_overclock_test);
    }
    
    dir = point_direction(fromObj.x, fromObj.y, toObj.x, toObj.y);
    len = point_distance(fromObj.x, fromObj.y, toObj.x, toObj.y);
    
    for (i = 0; i &lt; 12; i += 1)
    {
        dr = random(360);
        dx = fromObj.x + lengthdir_x(10, dr);
        dy = fromObj.y - 12 + lengthdir_y(14, dr);
        col = merge_color(c_aqua, c_black, 0.9 - random(0.1)); //(i * 0.13));
        sl = 0.2 + random(0.1);
        draw_sprite_ext(s_sine64, 0, dx, dy, sl, sl, 0, col, 1);
    }
    
    if (false)
    {
    for (i = 0; i &lt; 12; i += 1)
    {
        dr = random(360);
        dx = toObj.x + lengthdir_x(10, dr);
        dy = toObj.y - 12 + lengthdir_y(14, dr);
        col = merge_color(c_aqua, c_black, 0.9 - random(0.1)); //(i * 0.13));
        sl = 0.2 + random(0.1);
        draw_sprite_ext(s_sine64, 0, dx, dy, sl, sl, 0, col, 1);
    }
    }
    
    
    for (i = 0; i &lt; 4; i += 1)
    {
        col = merge_color(c_aqua, c_black, 0.9 - random(0.1)); //(i * 0.13));
        draw_set_color(col);
        rad = (i + 1) * 2;
        //draw_circle(x, y, rad, 0);
        xl = fromObj.x + lengthdir_x(len, dir);
        yl = fromObj.y - 12 + lengthdir_y(len, dir);
        draw_line_width(fromObj.x, fromObj.y - 12, xl, yl, rad * 1);
    }
    
    cou += 1;
    if (cou &gt; laserShow) 
    {
        cou = 0;
        state = 1; 
        ind += 1;
        stage += 1;
        if (ind &gt;= ds_list_size(list)) 
        {
            
            draw_set_blend_mode(bm_normal);
            instance_destroy();
        }
        //Switch
    }
}

if (state == 1)
{
    cou += 1;
    if (cou &gt; laserGap)
    {
        
        fromObj = toObj;
        state = 0;
        cou = 0;
    }
}
draw_set_blend_mode(bm_normal);
</string>

        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw
dx = x;
dy = y;

sy = scaleY;

//wiggle jelly

draw_set_color(c_green);
draw_set_alpha(0.33);
//Draw bottom
draw_primitive_begin(pr_trianglefan);
draw_vertex(dx + jellyX0[jellyMid], dy + mean(jellyY0[0], jellyY0[jellyPoints - 1]));
for (i = 0; i &lt; jellyPoints; i += 1)
{
    draw_vertex(dx + jellyX0[i], dy + jellyY0[i]);
}
draw_primitive_end();

if (dad != -999)
{
    with (dad) event_user(0);
}

//Draw top
draw_primitive_begin(pr_trianglefan);
draw_vertex(dx + jellyX1[jellyMid], dy + mean(jellyY1[0] * sy, jellyY1[jellyPoints - 1] * sy));
for (i = 0; i &lt; jellyPoints; i += 1)
{
    draw_vertex(dx + jellyX1[i], dy + jellyY1[i] * sy);
}
draw_primitive_end();

//Draw sides
for (i = 0; i &lt; jellyPoints; i += 1)
{
    if (i != jellyPoints - 1)
    {
        if (i == 0) jy10 = dy + jellyY1[i]; else jy10 = dy + jellyY1[i] * sy;
        if (i == jellyPoints - 1) jy11 = dy + jellyY1[i + 1]; else jy11 = dy + jellyY1[i + 1] * sy;
        draw_primitive_begin(pr_trianglefan);
        draw_vertex(dx + jellyX1[i], jy10); //dy + jellyY1[i] * sy);
        draw_vertex(dx + jellyX1[i + 1], jy11); //dy + jellyY1[i + 1] * sy);
        draw_vertex(dx + jellyX0[i + 1], dy + jellyY0[i + 1]);
        draw_vertex(dx + jellyX0[i], dy + jellyY0[i]);
        draw_primitive_end();
    }
}


//Draw all lines
draw_set_alpha(0.66);
for (i = 0; i &lt; jellyPoints; i += 1)
{
    //Draw top to bot lines
    if (i == jellyMid)
        draw_line(dx + jellyX0[i], dy + jellyY0[i], dx + jellyX1[i], dy + jellyY1[i] * sy);
    if (i == 0 || i == jellyPoints - 1) //No scale
        draw_line(dx + jellyX0[i], dy + jellyY0[i], dx + jellyX1[i], dy + jellyY1[i]);
    //Bottom and top lines
    if (i != jellyPoints - 1)
    {
        draw_line(dx + jellyX0[i], dy + jellyY0[i], dx + jellyX0[i + 1], dy + jellyY0[i + 1]);
        draw_line(dx + jellyX1[i], dy + jellyY1[i] * sy, dx + jellyX1[i + 1], dy + jellyY1[i + 1] * sy);
    }
    
}
draw_set_alpha(1);

scaleY += scaleYSpd;
scaleY = min(scaleY, 1);
</string>

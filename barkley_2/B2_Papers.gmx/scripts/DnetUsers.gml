else if (argument[0] == "sprite") // DnetUsers("sprite", username);
{
    var ind = ds_list_find_index(global.dslDnetName, argument[1]);
    if (ind == -1) return sDNETAvatarL;
    return global.dslDnetSprite[| ind];
}
else if (argument[0] == "sub") // DnetUsers("sub", username);
{
    var ind = ds_list_find_index(global.dslDnetName, argument[1]);
    if (ind == -1) return 0;
    return global.dslDnetSub[| ind];
}
else if (argument[0] == "posts") // DnetUsers("posts", username);
{
    var ind = ds_list_find_index(global.dslDnetName, argument[1]);
    if (ind == -1) return 0;
    return global.dslDnetPost[| ind];
}
else if (argument[0] == "status") // DnetUsers("status", username);
{
    var ind = ds_list_find_index(global.dslDnetName, argument[1]);
    if (ind == -1) return 0;
    return global.dslDnetStatus[| ind];
}
else if (argument[0] == "signature") // DnetUsers("status", username);
{
    var ind = ds_list_find_index(global.dslDnetName, argument[1]);
    if (ind == -1) return 0;
    return global.dslDnetSignature[| ind];
}
else if (argument[0] == "pips") // DnetUsers("pips", username);
{
    var ind = ds_list_find_index(global.dslDnetName, argument[1]);
    if (ind == -1) return 0;
    var cou = global.dslDnetPost[| ind];
    if (cou < 10) return 0;
    else if (cou < 50) return 1;
    else if (cou < 100) return 2;
    else if (cou < 250) return 3;
    else if (cou < 500) return 4;
    else if (cou < 1000) return 5;
    else if (cou < 5000) return 6;
    else return 7;
}
else if (argument[0] == "add")
{
    ds_list_add(global.dslDnetName, argument[1]);
    ds_list_add(global.dslDnetSprite, argument[2]);
    ds_list_add(global.dslDnetSub, argument[3]);
    ds_list_add(global.dslDnetPost, argument[4]);
    ds_list_add(global.dslDnetStatus, argument[5]);
    ds_list_add(global.dslDnetSignature, argument[6]);
}
else if (argument[0] == "init")
{
    global.dslDnetName = ds_list_create();
    global.dslDnetSprite = ds_list_create();
    global.dslDnetSub = ds_list_create();
    global.dslDnetPost = ds_list_create();
    global.dslDnetStatus = ds_list_create(); // 0 = Regular, 1 = Primo, 2 = Mod, 3 = Banned
    global.dslDnetSignature = ds_list_create();
    DnetUsers("data");
}

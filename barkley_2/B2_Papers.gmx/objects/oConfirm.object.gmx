            <string>/// Create
soundHoverLight = "sn_debug_one"; // Hover over buttons
soundClickExit = "sn_debug_three"; // Click exit
givTxt = "Buy Monofilament Jerkin for {5000?";
givTit = "NULL";
givPct = 0;
givSpd = 0.5; spd = givSpd;
givSel = 0;
givExe = -1;
givExt = 0;

givWid = 384 + 1; //to be safe add 1
givHov[0] = 0;
givHov[1] = 0;
givX[0] = x + floor(givWid / 2) + 10 - 3;
givX[1] = x + floor(givWid / 2) - 10 - 81 + 2;

// Text, Option 0, Option 1,

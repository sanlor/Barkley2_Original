            <string>/// GIVE
pctBgn = min(givPct * 2, 1);
pctTxt = clamp((givPct * 2) - 1, 0, 1);

draw_sprite_ext(s1x1, 0, view_xview[0], view_yview[0], SCREEN_WIDTH, SCREEN_HEIGHT, 0, c_black, 0.75 * pctBgn);

y = 240 / 2;
givHei = (2 * 12) + 44; //was (4 * 12) + 44;
givHei -= 3; //gz add
givHei = round(givHei * pctBgn);
y -= givHei / 2;

x = view_xview[0];
y += view_yview[0];
x = floor(x);
y = floor(y);

draw_spr_tiled(x, y, x + givWid, y + givHei, global.dialogBG, 0, 0.75 * pctBgn);

draw_set_valign(1);
draw_set_halign(1);
draw_set_font(global.fn_1);
draw_set_alpha(pctTxt);
draw_set_color(c_white);
txtWid = string_width_ext(givTxt, -1, 280);
draw_text_ext(x + floor(384 / 2), y + round(givHei * .34), givTxt, -1, 340);
draw_set_alpha(1);
draw_set_valign(0);
draw_set_halign(0);

dy = y + round(givHei * .61);
for (i = 0; i &lt; 2; i += 1) 
{
    if (givSel == i) draw_sprite_ext(sButtonPapers, 1, x + givX[i], dy, 1, 1, 0, butHilCol, givHov[i] * pctTxt);
    draw_sprite_ext(sButtonPapers, 5 - i, x + givX[i], dy, 1, 1, 0, merge_color(c_ltgray, butTxtCol, givHov[i]), pctTxt);
}

// Cursor
if (givSel == -1) curSub = 0; else curSub = 1;
curCol = merge_color(c_white, butTxtCol, curHov);
curHov -= dt_sec() / butSpd;
curHov = clamp(curHov, 0, 1);
if (global.keyinput_type != KEYINPUT_GAMEPAD) draw_sprite_ext(s_curs2, curSub, mouse_x, mouse_y, 1, 1, 0, curCol, 1);
</string>

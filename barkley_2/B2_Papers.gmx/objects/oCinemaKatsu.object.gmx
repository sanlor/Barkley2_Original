          <argument>
            <kind>1</kind>
            <string>/// Shoot ball

// Uschii - Pass
i = 0;
if (act[i])
{
    sub[i] = 1;
    prg[i] += dt_sec() / spd[i];
    if (prg[i] &gt; 0.5) sub[i] = 2;
    if (one[i] == 0) { bal[i] = 0; one[i] = 1; Trajectory("create", usx + 21, usy - 7, 15, 0, 0.5, ktx - 12, kty + 1, 15); }
    if (prg[i] &gt; 1) { act[i] = 0; prg[i] = 0; sub[i] = 3; one[i] = 0; }
}
if (bal[i] == 0 &amp;&amp; sub[i] == 0) sub[i] = 2;

// Katsu - Shoot
i = 1;
if (act[i])
{
    sub[i] = 1;
    prg[i] += dt_sec() / spd[i];
    if (abs(0.5 - prg[i]) &lt; 0.1) sub[i] = 2;
    if (sub[i] == 2 &amp;&amp; one[i] == 0) { one[i] = 1; bal[i] = 0; Trajectory("create", ktx, kty - 2 - 10, 52, 32, 1, ktx - (sht * xsl[i]), kty + shtY - 2, 100); }
    if (prg[i] &gt; 1) { act[i] = 0; prg[i] = 0; sub[i] = 0; one[i] = 0; }
}
else sub[i] = 0;
if (bal[i] == 0 &amp;&amp; sub[i] == 0) sub[i] = 3;
</string>

            <string>/// Draw
draw_sprite(sprite2, 0, 0, 0);

// DEBUG
if (keyboard_check_pressed(vk_space)) { bbrHit = 1; bbrPct -= 0.03 + random(0.03); }
if (keyboard_check_pressed(vk_shift)) { bbrHit = 0; bbrPct = 1; bbrPctLst = 1; }

// Bootstrap code
//if (string(Quest("marqueeBossBar")) != "0") 
//{
//    bbrPct = Quest("marqueeBossBarHealth");
//    bbrTxt = Quest("marqueeBossBar");
//}

// Real code
draw_set_font(global.fn_3);
bbrPct = clamp(bbrPct, 0, 1);
bbrRct = abs(1 - bbrPct);
bbrHit = Goto(bbrHit, 0, dt_sec() * 4);
if (bbrHit == 0) bbrPctLst = bbrPct;

// Background
if (surface_exists(bbrSur) == 0) bbrSur = surface_create(bbrW, bbrH);
surface_set_target(bbrSur);
draw_clear(merge_color(bbrColBak, c_white, bbrHit * bbrMrg));
draw_set_color(merge_color(bbrTxtColBak, c_white, bbrHit * bbrMrg));
draw_text(-bbrTxtWid + bbrTxtPos, 1, string_repeat(bbrTxt, 5));
bbrTxtPos += dt_sec() * bbrTxtSpd;
if (abs(bbrTxtPos) &gt; bbrTxtWid) bbrTxtPos -= bbrTxtWid;
surface_reset_target();

// Foreground
if (surface_exists(bbrFor) == 0) bbrFor = surface_create(bbrW, bbrH);
surface_set_target(bbrFor);
draw_clear(merge_color(bbrColFil, c_white, bbrHit * bbrMrg));
draw_set_color(merge_color(bbrTxtCol, c_white, bbrHit * bbrMrg));
draw_text(-bbrTxtWid + bbrTxtPos, 1, string_repeat(bbrTxt, 5));
bbrTxtPos += dt_sec() * bbrTxtSpd;
if (abs(bbrTxtPos) &gt; bbrTxtWid) bbrTxtPos -= bbrTxtWid;
surface_reset_target();

// Draw
draw_surface(bbrSur, bbrX, bbrY);
draw_surface_part(bbrFor, floor(bbrW * bbrRct), 0, bbrW - floor(bbrW * bbrRct), bbrH, bbrX + floor(bbrW * bbrRct), bbrY);
draw_sprite_ext(s1x1, 0, bbrX + (abs(1 - bbrPctLst) * bbrW), bbrY, abs(bbrPct - bbrPctLst) * bbrW, bbrH, 0, c_white, bbrHit * 2);
</string>

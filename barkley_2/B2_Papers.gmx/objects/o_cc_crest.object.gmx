            <string>// Dont run the event after this for this character //
o_cc_data.event_finished_crest = true;

// Phase setup //
phase[0] = 18;
phase[1] = 1;
phase[2] = 0;

// Phase skipping //
phase_skip_time = 0;
phase_skip_to = 0;
phase_skip = 0;

// Textbox setup //

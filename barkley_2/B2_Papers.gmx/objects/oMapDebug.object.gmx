        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Map debug
if keyboard_check_pressed(ord('Q')) then toggle = !toggle;

//hide = Goto(hide, hideToggle, dt_sec() / 0.33);
if (instance_exists(oMap) == 0)
    draw_sprite_ext(s_curs2, 0, mouse_x, mouse_y, 1, 1, 0, c_white, 1);

if (toggle == 0) exit;

drx = 2;
dry = 2;
draw_set_font(global.fn_debug);
draw_set_color(c_fuchsia);
draw_text(drx, dry, "Press Q to toggle debug, press W to bring up Map screen");
dry += 9;

for (i = 0; i &lt; ds_list_size(lst); i += 1)
{
    var qst = ds_list_find_value(lst, i);
    if (scr_mouse_rect(drx, dry, drx + 80, dry + 8))
    {
        draw_set_color(c_lime);
        if (mouse_check_button_pressed(mb_left)) 
        {
            if keyboard_check(vk_shift) scr_quest_set_state(qst, Quest(qst) + 50);
            else scr_quest_set_state(qst, Quest(qst) + 1);
        }
        if (mouse_check_button_pressed(mb_right)) 
        {
            if keyboard_check(vk_shift) scr_quest_set_state(qst, Quest(qst) - 50);
            else scr_quest_set_state(qst, Quest(qst) - 1);
        }
    }
    else draw_set_color(c_fuchsia);
    draw_text(drx, dry, qst);
    draw_text(drx + 65, dry, "=");
    draw_text(drx + 70, dry, Quest(qst));
    dry += 9;
}

// Maps that you have
draw_set_halign(2);
drx = 384 - 2;
dry = 2;
draw_set_font(global.fn_debug);
draw_set_color(c_fuchsia);
draw_text(drx, dry, "Maps Hoopz has...");
dry += 9;
for (i = 0; i &lt; ds_list_size(global.mapNam); i += 1)
{
    var qst = ds_list_find_value(global.mapNam, i);
    if (scr_mouse_rect(drx - 80, dry, drx, dry + 8))
    {
        draw_set_color(c_lime);
        if (mouse_check_button_pressed(mb_left)) Map("gain", qst);
        if (mouse_check_button_pressed(mb_right)) Map("lose", qst);
    }
    else 
    {
        if (Map("have", qst)) draw_set_color(c_white);
        else draw_set_color(c_gray);
    }
    draw_text(drx, dry, qst);
    dry += 9;
}
draw_set_halign(0);
</string>

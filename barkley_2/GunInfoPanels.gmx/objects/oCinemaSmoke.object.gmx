        {
            smkXScl[i, h] = 0.6 - (q * 0.15) + random(0.2);
            smkYScl[i, h] = 0.6 - (q * 0.15) + random(0.2);
            smkX[i, h] = -1 + floor(random(3));
            smkY[i, h] = -1 + floor(random(3));
            smkX[i, h] *= 1 + (q / 2);
            smkXSpd[i, h] = -0.5 + random(1);
            smkYSpd[i, h] = -(1 + random(2));
            smkRot[i, h] = random(360);
        }
    }
}

          <argument>
            <kind>1</kind>
            <string>// Pause //
if paused() then exit;

bubbleCount += dt_sec();
while (bubbleCount &gt; 1 / bubblePerSecond)
{
    bubbleCount -= 1 / bubblePerSecond;
    bubble = instance_create(x, y, o_mg_diving_bubble);
    if (instance_exists(bubble))
    {
        bubble.lifespan /= 3;
        bubble.lifespanTotal /= 3;
        bubble.depth = depth + 1;
    }
}

// Move //
if ignite &gt; 0 then
    {
    // Tick down //
    ignite -= dt();
    
    // Event //
    if ignite &lt;= 10 then
        {
        if instance_exists(o_mg_diving_player) then 
            {
            motion_add(point_direction(x, y, o_mg_diving_player.x, o_mg_diving_player.y), 0.15);
            if speed &gt; 2 then speed = 2;  
            
            x_speed += hspeed;
            y_speed += vspeed;
            hspeed = 0;
            vspeed = 0;

            image_speed = 2.4 * dt();
            }
        }
    }

// Move //    
x += x_speed * dt() * 3;
y += y_speed * dt() * 3;
    
// Image //
if ignite &lt;= 10 and image_index &gt; 2.5 then image_index = 1;

// Blow up on contact //
if place_meeting(x, y, o_mg_diving_player) or !place_free(x, y) then instance_destroy();

// Angle //
image_angle = point_direction(0, 0, x_speed, y_speed);
</string>

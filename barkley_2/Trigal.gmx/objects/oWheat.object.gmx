          <argument>
            <kind>1</kind>
            <string>//was 16x64
tim -= global.timSec;
if (tim &lt;= 0)
{
    tim = 1;
    sub = !sub;
}
dir = oCamera3D.dir;//point_direction(oCamera3D.x, oCamera3D.y, x, y); //oCamera3D.dir - 90;
x0 = x + lengthdir_x(wid, dir - 270);
y0 = y + lengthdir_y(wid, dir - 270);
x1 = x + lengthdir_x(wid, dir + 270);
y1 = y + lengthdir_y(wid, dir + 270);

var handpushing = false;
if(oCinemaWheat.camInd==2 )
{
    if(point_distance(x,y,oCamera3D.x,oCamera3D.y)&lt;14)
    {
        handpushing = true;
        pushed +=1;
    }
}

if(oCinemaWheat.camInd==3 )
{
    if(point_distance(x,y,oWheatHoopz.x,oWheatHoopz.y)&lt;24)
    {
        handpushing = true;
    }
}

if(!handpushing &amp;&amp; pushed&gt;0){pushed-=1;}

var frame = tex[sub];//sub mod 4];
if(handpushing){frame = tex[2];}

if burning &gt; 0 then
    {
    burning -= 1;
    if burning &lt;= 0 then burned = true;
    if burning &lt; 120 then d3d_draw_wall(x0+pushed/4, y0, 64-pushed/8, x1+pushed/4, y1, 0-pushed/8, tex[4 + irandom(1)], 1, 1);
    }

if burned = true then d3d_draw_wall(x0+pushed/4, y0, 64-pushed/8, x1+pushed/4, y1, 0-pushed/8, tex[3], 1, 1);
else if burning &gt;= 120 or burning = -1 then d3d_draw_wall(x0+pushed/4, y0, 64-pushed/8, x1+pushed/4, y1, 0-pushed/8, frame, 1, 1);
//draw_set_alpha_test(false);

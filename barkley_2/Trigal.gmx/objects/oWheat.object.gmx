          <argument>
            <kind>1</kind>
            <string>/// Create
x += 16;
y += 16;
wid = 64 / 2;
var tx = sWheat;
var wheatFrm = irandom(3);
tex[0] = sprite_get_texture(tx, wheatFrm*3);
tex[1] = sprite_get_texture(tx, wheatFrm*3+1);
tex[2] = sprite_get_texture(tx, wheatFrm*3+2);
tex[3] = sprite_get_texture(sWheatBurned, wheatFrm);
tex[4] = sprite_get_texture(sWheatFire, 0);
tex[5] = sprite_get_texture(sWheatFire, 1);
sub = choose(0, 1);
tim = 1 + random(1);
burned = false;
burning = -1;
pushed = 0;

var dst = point_distance(x,y,1024,800);
var wheatAmnt = 2+irandom(3);

if(dst&gt;300){wheatAmnt =1;}
if(dst&gt;600){wheatAmnt = 0;}

if(wheatAmnt&gt;0)
{
    repeat(wheatAmnt)
    {
        var ob = instance_create(x,y,o_miniWheat);
        var dst = 1+ random(16);
        var dir = random(360);
        ob.x += lengthdir_x(dst,dir);
        ob.y += lengthdir_y(dst,dir);
    }
}
</string>
          </argument>
        </arguments>
      </action>
